[
    {
        "sno": 1,
        "slokaNo": 71,
        "sloka": "अन्यकालपरिहार्यमजस्त्रं तद् द्वयेन"
    },
    {
        "sno": 2,
        "slokaNo": 28,
        "sloka": "अन्ययाऽन्यवनितागतचित्तं चित्तनाथमभि"
    },
    {
        "sno": 3,
        "slokaNo": 83,
        "sloka": "अप्रभूतमतनीयसि तन्वी काञ्चिधाम्नि"
    },
    {
        "sno": 4,
        "slokaNo": 14,
        "sloka": "अप्रसन्नमपराद्धरि पत्यौ कोपदीप्त"
    },
    {
        "sno": 5,
        "slokaNo": 27,
        "sloka": "अर्पितं रसितवत्यपि नामग्राहमन्ययुवते"
    },
    {
        "sno": 6,
        "slokaNo": 62,
        "sloka": "अम्बरं विनयतः प्रियपाणेर्योषितश्च करयोः"
    },
    {
        "sno": 7,
        "slokaNo": 43,
        "sloka": "अंशुकं हृतवता तनुबाहुस्वस्तिकापिहित"
    },
    {
        "sno": 8,
        "slokaNo": 20,
        "sloka": "आगतानगणितप्रतियातान् वल्लभानभिसिसार"
    },
    {
        "sno": 9,
        "slokaNo": 36,
        "sloka": "आननैर्विचकसे हृषिताभिर्वल्लभानभि तनूभिरभावि"
    },
    {
        "sno": 10,
        "slokaNo": 59,
        "sloka": "आमृशद्भिरभितो वलिवीचीर्लोलमान"
    },
    {
        "sno": 11,
        "slokaNo": 65,
        "sloka": "आयताङ्गुलिरभूदतिरिक्तः सुभ्रुवां क्रशिमशालिनि"
    },
    {
        "sno": 12,
        "slokaNo": 56,
        "sloka": "आवृतान्यपि निरन्तरमुच्चैर्योषितामुरसिज"
    },
    {
        "sno": 13,
        "slokaNo": 64,
        "sloka": "आशु लङ्घितवतीष्टकराग्रे नीविमर्ध"
    },
    {
        "sno": 14,
        "slokaNo": 89,
        "sloka": "आस्तृतेऽभिनवपल्लवपुष्पैरप्यनारत"
    },
    {
        "sno": 15,
        "slokaNo": 74,
        "sloka": "आहतं कुचतटेन तरुण्याः साधु"
    },
    {
        "sno": 16,
        "slokaNo": 91,
        "sloka": "इति मदमदनाभ्यां रागिणः स्पष्टरागानन"
    },
    {
        "sno": 17,
        "slokaNo": 77,
        "sloka": "ईदृशस्य भवतः कथमेतल्लाघवं मुहुरितीव"
    },
    {
        "sno": 18,
        "slokaNo": 42,
        "sloka": "उत्तरीयविनयात्त्रपमाणा रुन्धती किल"
    },
    {
        "sno": 19,
        "slokaNo": 32,
        "sloka": "उद्धतैरिव परस्परसङ्गादीरितान्युभयतः"
    },
    {
        "sno": 20,
        "slokaNo": 76,
        "sloka": "उद्धतैर्निभृतमेकमनेकैश्छेदवन्मृगदृशा"
    },
    {
        "sno": 21,
        "slokaNo": 67,
        "sloka": "ऊरुमूलचपलेक्षणमघ्नन् यैर्वतंसकुसुमैः"
    },
    {
        "sno": 22,
        "slokaNo": 10,
        "sloka": "कस्यचित्समदनं मदनीयप्रेयसीवदन"
    },
    {
        "sno": 23,
        "slokaNo": 73,
        "sloka": "कान्तया सपदि कोऽप्युपगूढः प्रौढपाणि"
    },
    {
        "sno": 24,
        "slokaNo": 4,
        "sloka": "कापिशायनसुगन्धि विघूर्णन्नुन्मदोऽधिशयितुं"
    },
    {
        "sno": 25,
        "slokaNo": 57,
        "sloka": "कामिनामसकलानि विभुग्नैः स्वेदवारिमृदुभिः"
    },
    {
        "sno": 26,
        "slokaNo": 61,
        "sloka": "कामिनः कृतरतोत्सवकालक्षेपमाकुलवधूकर"
    },
    {
        "sno": 27,
        "slokaNo": 30,
        "sloka": "कुर्वता मुकुलिताक्षियुगानामङ्गसादमवसादित"
    },
    {
        "sno": 28,
        "slokaNo": 54,
        "sloka": "केनचिन्मधुरमुल्बणरागं बाष्पतप्तमधिकं विरहेषु"
    },
    {
        "sno": 29,
        "slokaNo": 29,
        "sloka": "कोपवत्यनुनयानगृहीत्वा प्रागथो मधुमदाऽऽहित"
    },
    {
        "sno": 30,
        "slokaNo": 3,
        "sloka": "क्रान्तकान्तवदनप्रतिबिम्बे भग्नबालसहकार"
    },
    {
        "sno": 31,
        "slokaNo": 34,
        "sloka": "क्षीबतामुपगतास्वनुवेलं तासु रोषपरितोष"
    },
    {
        "sno": 32,
        "slokaNo": 31,
        "sloka": "गण्डभित्तिषु पुरा सदृशीषु व्याञ्जि नाञ्चित"
    },
    {
        "sno": 33,
        "slokaNo": 63,
        "sloka": "ग्रन्थिमुद्ग्रथयितुं हृदयदेशे वाससः"
    },
    {
        "sno": 34,
        "slokaNo": 66,
        "sloka": "चक्रुरेव ललनोरुषु राजीः स्पर्शलोभवश"
    },
    {
        "sno": 35,
        "slokaNo": 33,
        "sloka": "चारुता वपुरभूषयदासां तामनूननव"
    },
    {
        "sno": 36,
        "slokaNo": 19,
        "sloka": "छादितः कथमपि त्रपयाऽन्तर्यः प्रियं"
    },
    {
        "sno": 37,
        "slokaNo": 23,
        "sloka": "दत्तमात्तमदनं दयितेन व्याप्तमातिशयिकेन"
    },
    {
        "sno": 38,
        "slokaNo": 6,
        "sloka": "दत्तमिष्टतमया मधु पत्युर्बाढमाप पिबतो"
    },
    {
        "sno": 39,
        "slokaNo": 47,
        "sloka": "दीपितस्मरमुरस्युपपीडं वल्लभे घनमभिष्वज"
    },
    {
        "sno": 40,
        "slokaNo": 68,
        "sloka": "धैर्यमुल्बणमनोभवभावा वामतां च वपुरर्पित"
    },
    {
        "sno": 41,
        "slokaNo": 50,
        "sloka": "न स्म माति वपुषः प्रमदानामन्तरिष्टतम"
    },
    {
        "sno": 42,
        "slokaNo": 53,
        "sloka": "पल्लवोपमितिसाम्यसपक्षं दष्टवत्यधर"
    },
    {
        "sno": 43,
        "slokaNo": 26,
        "sloka": "पानधौतनवयावकरागं सुभ्रुवो निभृत"
    },
    {
        "sno": 44,
        "slokaNo": 69,
        "sloka": "पाणिरोधमविरोधितवाञ्छं भर्त्सनाश्च"
    },
    {
        "sno": 45,
        "slokaNo": 46,
        "sloka": "पीडिते पुर उरःप्रतिपेषं भर्तरि स्तनयुगेन"
    },
    {
        "sno": 46,
        "slokaNo": 9,
        "sloka": "पीतवत्यभिमते मधुतुल्यस्वादमोष्ठरुचकं"
    },
    {
        "sno": 47,
        "slokaNo": 11,
        "sloka": "पीतशीधुमधुरैर्मिथुनानामाननैः परिहृतं"
    },
    {
        "sno": 48,
        "slokaNo": 12,
        "sloka": "प्रातिभं त्रिसरकेण गतानां वक्रवाक्य"
    },
    {
        "sno": 49,
        "slokaNo": 60,
        "sloka": "प्राप्य नाभिनदमज्जनमाशु प्रस्थितं"
    },
    {
        "sno": 50,
        "slokaNo": 80,
        "sloka": "प्राप्य मन्मथरसादतिभूमिं दुर्वहस्तन"
    },
    {
        "sno": 51,
        "slokaNo": 78,
        "sloka": "प्राप्यते स्म गतचित्रकचित्रैश्चित्रमार्द्र"
    },
    {
        "sno": 52,
        "slokaNo": 82,
        "sloka": "प्रेक्षणीयकमिव क्षणमासन् ह्रीविभङ्गुर"
    },
    {
        "sno": 53,
        "slokaNo": 72,
        "sloka": "बाहुपीडनकचग्रहणाभ्यामाहतेन नखदन्त"
    },
    {
        "sno": 54,
        "slokaNo": 8,
        "sloka": "बिभ्रतौ मधुरतामतिमात्रं रागिभिर्युग"
    },
    {
        "sno": 55,
        "slokaNo": 5,
        "sloka": "बिम्बितं भृतपरिस्रुति जानन् भाजने"
    },
    {
        "sno": 56,
        "slokaNo": 86,
        "sloka": "भातु नाम सुदृशां दशनाऽङ्कः पाटलो"
    },
    {
        "sno": 57,
        "slokaNo": 17,
        "sloka": "मद्यमन्दविगलत्त्रपमीषच्चक्षुरुन्मिषितपक्ष्म दधत्या"
    },
    {
        "sno": 58,
        "slokaNo": 21,
        "sloka": "मा पुनस्तमभिसीसरमागस्कारिणं मदविमोहित"
    },
    {
        "sno": 59,
        "slokaNo": 25,
        "sloka": "मानभङ्गपटुना सुरतेच्छां तन्वता प्रथयता"
    },
    {
        "sno": 60,
        "slokaNo": 84,
        "sloka": "मृष्टचन्दनविशेषकभक्तिर्भ्रष्टभूषणकदर्थितमाल्यः"
    },
    {
        "sno": 61,
        "slokaNo": 51,
        "sloka": "यत् प्रियव्यतिकराद्वनितानमङ्गजेन पुलकेन"
    },
    {
        "sno": 62,
        "slokaNo": 79,
        "sloka": "यद्यदेव रुरुचे रुचिरेभ्यः सुभ्रुवो रहसि"
    },
    {
        "sno": 63,
        "slokaNo": 18,
        "sloka": "या कथञ्चन सखीवचनेन प्रागभिप्रियतमं"
    },
    {
        "sno": 64,
        "slokaNo": 40,
        "sloka": "यूनि रागतरलैरपि तिर्यक्पातिभिः श्रुतिगुणेन"
    },
    {
        "sno": 65,
        "slokaNo": 90,
        "sloka": "योषितामतितरां नखलूनं गात्रमुज्ज्वलतया"
    },
    {
        "sno": 66,
        "slokaNo": 85,
        "sloka": "योषितः पतितकाञ्चनकाञ्चौ मोहनाऽतिरभसेन"
    },
    {
        "sno": 67,
        "slokaNo": 37,
        "sloka": "रूपमप्रतिविधानमनोज्ञं प्रेम कार्यमनपेक्ष्य विकासि"
    },
    {
        "sno": 68,
        "slokaNo": 55,
        "sloka": "रेचितं परिजनेन महीयः केवलाऽभिरतदम्पति धाम"
    },
    {
        "sno": 69,
        "slokaNo": 24,
        "sloka": "लब्धसौरभगुणो मदिराणामङ्गनास्यचषकस्य च गन्धः"
    },
    {
        "sno": 70,
        "slokaNo": 38,
        "sloka": "लीलयैव सुतनोस्तुलयित्वा गौरवाढ्यमपि लावणिकेन"
    },
    {
        "sno": 71,
        "slokaNo": 70,
        "sloka": "वारणार्थपदगद्गदवाचामीर्ष्यया मुहुरपत्रपया"
    },
    {
        "sno": 72,
        "slokaNo": 88,
        "sloka": "विश्रमार्थमुपगूढमजस्रं यत्प्रियैः प्रथम"
    },
    {
        "sno": 73,
        "slokaNo": 41,
        "sloka": "सङ्कथेच्छुरभिधातुमनीशा सम्मुखी न"
    },
    {
        "sno": 74,
        "slokaNo": 81,
        "sloka": "सङ्गताभिरुचितैश्चलिताऽपि प्रागमुच्यत"
    },
    {
        "sno": 75,
        "slokaNo": 44,
        "sloka": "सञ्जहार सहसा परिरब्धप्रेयसीषु विरहय्य"
    },
    {
        "sno": 76,
        "slokaNo": 1,
        "sloka": "सज्जितानि सुरभीण्यथ यूनामुल्लसन्नयन"
    },
    {
        "sno": 77,
        "slokaNo": 15,
        "sloka": "सन्तमेव चिरमप्रकृतत्वादप्रकाशितमदिद्युतदङ्गे"
    },
    {
        "sno": 78,
        "slokaNo": 48,
        "sloka": "सम्प्रवेष्टुमिव योषित ईषुः श्लिष्यतां"
    },
    {
        "sno": 79,
        "slokaNo": 16,
        "sloka": "सावशेषपदमुक्तमुपेक्षा स्रस्तमाल्य"
    },
    {
        "sno": 80,
        "slokaNo": 75,
        "sloka": "सीत्कृतानि मणितं करुणोक्तिः स्निग्ध"
    },
    {
        "sno": 81,
        "slokaNo": 87,
        "sloka": "सुभ्रुवामधिपयोधरपीठं पीडनैस्त्रुटित"
    },
    {
        "sno": 82,
        "slokaNo": 2,
        "sloka": "सोपचारमुपशान्तविचारं सानुतर्ष"
    },
    {
        "sno": 83,
        "slokaNo": 58,
        "sloka": "सोष्मणः स्तनशिलाशिखराग्रादात्तघर्म"
    },
    {
        "sno": 84,
        "slokaNo": 49,
        "sloka": "स्नेहनिर्भरमधत्त वधूनामार्द्रतां वपु"
    },
    {
        "sno": 85,
        "slokaNo": 39,
        "sloka": "स्पर्शभाजि विशदच्छविचारौ कल्पिते"
    },
    {
        "sno": 86,
        "slokaNo": 45,
        "sloka": "स्रंसमानमुपयन्तरि वध्वाः श्लिष्टवत्युप"
    },
    {
        "sno": 87,
        "slokaNo": 7,
        "sloka": "स्वादनेन सुतनोरविचारादोष्ठतः समचरिष्ट"
    },
    {
        "sno": 88,
        "slokaNo": 35,
        "sloka": "शङ्कयाऽन्ययुवतौ वनिताभिः प्रत्यभेदि"
    },
    {
        "sno": 89,
        "slokaNo": 13,
        "sloka": "हावहारि हसितं वचनानां कौशलं दृशि"
    },
    {
        "sno": 90,
        "slokaNo": 52,
        "sloka": "ह्रीभरादवनतं परिरम्भे रागवानवटु"
    },
    {
        "sno": 91,
        "slokaNo": 22,
        "sloka": "ह्रीविमोहमहरद्दयितानामन्तिकं रतिसुखाय"
    }
]