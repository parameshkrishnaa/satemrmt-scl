[
    {
        "sno": 1,
        "slokaNo": 52,
        "sloka": "अकृत्वा हेलया पादमुच्चै"
    },
    {
        "sno": 2,
        "slokaNo": 53,
        "sloka": "अङ्काधिरोपितमृगश्चन्द्रमा"
    },
    {
        "sno": 3,
        "slokaNo": 113,
        "sloka": "अज्ञातदोषैर्दोषज्ञैरुद्दूष्योभय"
    },
    {
        "sno": 4,
        "slokaNo": 5,
        "sloka": "अध्यासामासुरुत्तुङ्ग"
    },
    {
        "sno": 5,
        "slokaNo": 90,
        "sloka": "अनल्पत्वात्प्रधानत्वाद् वंशस्येव"
    },
    {
        "sno": 6,
        "slokaNo": 27,
        "sloka": "अनिर्लोडितकार्यस्य"
    },
    {
        "sno": 7,
        "slokaNo": 112,
        "sloka": "अनुत्सूत्रपदन्यासा सद्वृत्तिः"
    },
    {
        "sno": 8,
        "slokaNo": 44,
        "sloka": "अन्यदा भूषणं पुंसः क्षमा"
    },
    {
        "sno": 9,
        "slokaNo": 62,
        "sloka": "अन्यदुच्छृङ्खलं सत्वमन्यच्छास्त्र"
    },
    {
        "sno": 10,
        "slokaNo": 91,
        "sloka": "अप्यनारभमाणस्य विभोरुत्पादिताः"
    },
    {
        "sno": 11,
        "slokaNo": 107,
        "sloka": "अमृतं नाम यत्सन्तो"
    },
    {
        "sno": 12,
        "slokaNo": 47,
        "sloka": "असम्पादयतः कश्चिदर्थं जाति"
    },
    {
        "sno": 13,
        "slokaNo": 30,
        "sloka": "आत्मोदयः परज्यानिर्द्वयं "
    },
    {
        "sno": 14,
        "slokaNo": 79,
        "sloka": "आरभन्तेऽल्पमेवाऽज्ञाः कामं"
    },
    {
        "sno": 15,
        "slokaNo": 40,
        "sloka": "आलप्यालमिदं बभ्रोर्यत्स"
    },
    {
        "sno": 16,
        "slokaNo": 17,
        "sloka": "आश्लेषलोलुपवधूस्तन"
    },
    {
        "sno": 17,
        "slokaNo": 118,
        "sloka": "इति विशकलितार्थामौद्धवीं"
    },
    {
        "sno": 18,
        "slokaNo": 67,
        "sloka": "इति संरम्भिणो वाणीर्बलस्य"
    },
    {
        "sno": 19,
        "slokaNo": 63,
        "sloka": "इन्द्रप्रस्थगमस्तावत्कारि मा"
    },
    {
        "sno": 20,
        "slokaNo": 10,
        "sloka": "उत्तिष्ठमानस्तु परो"
    },
    {
        "sno": 21,
        "slokaNo": 81,
        "sloka": "उदेतुमत्यजन्नीहां राजसु"
    },
    {
        "sno": 22,
        "slokaNo": 37,
        "sloka": "उपकर्त्राऽरिणा सन्धिर्न"
    },
    {
        "sno": 23,
        "slokaNo": 99,
        "sloka": "उपजापः कृतस्तेन"
    },
    {
        "sno": 24,
        "slokaNo": 80,
        "sloka": "उपायमास्थितस्यापि नश्यन्त्यर्थाः"
    },
    {
        "sno": 25,
        "slokaNo": 114,
        "sloka": "उपेयिवांसि कर्तारः"
    },
    {
        "sno": 26,
        "slokaNo": 20,
        "sloka": "ककुद्मिकन्यावक्त्रान्तर्वासलब्धाधि"
    },
    {
        "sno": 27,
        "slokaNo": 9,
        "sloka": "करदीकृतभूपालो भ्रातृभिर्जित्वरै"
    },
    {
        "sno": 28,
        "slokaNo": 89,
        "sloka": "करप्रचेयामुत्तुङ्गः प्रभुशक्तिं"
    },
    {
        "sno": 29,
        "slokaNo": 84,
        "sloka": "कृतापचारोऽपि परैरनाविष्कृत"
    },
    {
        "sno": 30,
        "slokaNo": 111,
        "sloka": "कृत्वा कृत्यविदस्तीर्थेष्वन्तः"
    },
    {
        "sno": 31,
        "slokaNo": 56,
        "sloka": "गुणानामायथातथ्यादर्थं"
    },
    {
        "sno": 32,
        "slokaNo": 6,
        "sloka": "गुरुद्वयाय गुरुणोरुभयोरथ"
    },
    {
        "sno": 33,
        "slokaNo": 16,
        "sloka": "घूर्णयन् मदिरास्वादमदपाटलित"
    },
    {
        "sno": 34,
        "slokaNo": 54,
        "sloka": "चतुर्थोपायसाध्ये तु रिपौ"
    },
    {
        "sno": 35,
        "slokaNo": 105,
        "sloka": "चिरादपि बलात्कारो बलिनः"
    },
    {
        "sno": 36,
        "slokaNo": 21,
        "sloka": "जगाद वदनच्छद्मपद्म"
    },
    {
        "sno": 37,
        "slokaNo": 3,
        "sloka": "जाज्वल्यमाना जगतः"
    },
    {
        "sno": 38,
        "slokaNo": 14,
        "sloka": "ततः सपत्नापनयस्मरणानुशय"
    },
    {
        "sno": 39,
        "slokaNo": 71,
        "sloka": "तथापि यन्मय्यपि गुरु"
    },
    {
        "sno": 40,
        "slokaNo": 95,
        "sloka": "तदीशितारं चेदीनां भवांस्तम"
    },
    {
        "sno": 41,
        "slokaNo": 88,
        "sloka": "तन्त्रावापविदा योगैर्मण्डला"
    },
    {
        "sno": 42,
        "slokaNo": 101,
        "sloka": "तस्य मित्राण्यमित्रास्ते"
    },
    {
        "sno": 43,
        "slokaNo": 109,
        "sloka": "तीक्ष्णा नारुन्तुदा बुद्धिः"
    },
    {
        "sno": 44,
        "slokaNo": 48,
        "sloka": "तुङ्गत्वमितरा नाद्रौ नेदं"
    },
    {
        "sno": 45,
        "slokaNo": 49,
        "sloka": "तुल्येऽपराधे स्वर्भानुर्भानुमन्तं"
    },
    {
        "sno": 46,
        "slokaNo": 31,
        "sloka": "तृप्तियोगः परेणाऽपि महिम्ना"
    },
    {
        "sno": 47,
        "slokaNo": 83,
        "sloka": "तेजः क्षमा वा नैकान्तं"
    },
    {
        "sno": 48,
        "slokaNo": 51,
        "sloka": "तेजस्विमध्ये तेजस्वी दवीयानपि"
    },
    {
        "sno": 49,
        "slokaNo": 38,
        "sloka": "त्वया विप्रकृतश्चैद्यो रुक्मिणीं"
    },
    {
        "sno": 50,
        "slokaNo": 39,
        "sloka": "त्वयि भौमं गते जेतुमरौत्सी"
    },
    {
        "sno": 51,
        "slokaNo": 18,
        "sloka": "दधत्सन्ध्यारुणव्योमस्फुरत्तारा"
    },
    {
        "sno": 52,
        "slokaNo": 7,
        "sloka": "द्योतितान्तःसभैः कुन्दकुड्मलाग्रदतः"
    },
    {
        "sno": 53,
        "slokaNo": 35,
        "sloka": "ध्रियते यावदेकोऽपि रिपुस्तावत्"
    },
    {
        "sno": 54,
        "slokaNo": 11,
        "sloka": "न दूये सात्वतीसूनुः"
    },
    {
        "sno": 55,
        "slokaNo": 86,
        "sloka": "नालम्बते दैष्टिकतां न"
    },
    {
        "sno": 56,
        "slokaNo": 64,
        "sloka": "निरुद्धवीवधासारप्रसारा गा"
    },
    {
        "sno": 57,
        "slokaNo": 68,
        "sloka": "निशम्य ताः शेषगवीरभिधातु"
    },
    {
        "sno": 58,
        "slokaNo": 61,
        "sloka": "नीतिरापदि यद् गम्यः परस्तन्म"
    },
    {
        "sno": 59,
        "slokaNo": 23,
        "sloka": "नैतल्लघ्वपि भूयस्या वचो"
    },
    {
        "sno": 60,
        "slokaNo": 46,
        "sloka": "पादाहतं यदुत्थाय मूर्धान"
    },
    {
        "sno": 61,
        "slokaNo": 76,
        "sloka": "प्रज्ञोत्साहवतः स्वामी-"
    },
    {
        "sno": 62,
        "slokaNo": 66,
        "sloka": "प्राप्यतां विद्युतां सम्पत्सम्पर्क"
    },
    {
        "sno": 63,
        "slokaNo": 19,
        "sloka": "प्रोल्लसत्कुण्डलप्रोतपद्मराग"
    },
    {
        "sno": 64,
        "slokaNo": 73,
        "sloka": "बह्वपि स्वेच्छया कामं"
    },
    {
        "sno": 65,
        "slokaNo": 82,
        "sloka": "बुद्धिशस्त्रः प्रकृत्यङ्गो"
    },
    {
        "sno": 66,
        "slokaNo": 100,
        "sloka": "बृहत्सहायः कार्यान्तं"
    },
    {
        "sno": 67,
        "slokaNo": 8,
        "sloka": "भवद्गिरामवसरप्रदानाय"
    },
    {
        "sno": 68,
        "slokaNo": 69,
        "sloka": "भारतीमाहितभरामथानुद्धत"
    },
    {
        "sno": 69,
        "slokaNo": 102,
        "sloka": "मखविघ्नाय सकलमित्थ"
    },
    {
        "sno": 70,
        "slokaNo": 43,
        "sloka": "मनागनभ्यावृत्या वा कामं"
    },
    {
        "sno": 71,
        "slokaNo": 29,
        "sloka": "मन्त्रो योध इवाधीरः"
    },
    {
        "sno": 72,
        "slokaNo": 106,
        "sloka": "मन्यसेऽरिवधः श्रेयान्"
    },
    {
        "sno": 73,
        "slokaNo": 12,
        "sloka": "मम तावन्मतमिदं श्रूयतामङ्ग"
    },
    {
        "sno": 74,
        "slokaNo": 104,
        "sloka": "महात्मानोऽनुगृह्णन्ति"
    },
    {
        "sno": 75,
        "slokaNo": 45,
        "sloka": "मा जीवन् यः परावज्ञा"
    },
    {
        "sno": 76,
        "slokaNo": 96,
        "sloka": "मा वेदि यदसावेको"
    },
    {
        "sno": 77,
        "slokaNo": 85,
        "sloka": "मृदुव्यवहितं तेजो"
    },
    {
        "sno": 78,
        "slokaNo": 74,
        "sloka": "म्रदीयसीमपि घनामनल्पगुण"
    },
    {
        "sno": 79,
        "slokaNo": 116,
        "sloka": "य इहात्मविदो विपक्षमध्ये सहसं"
    },
    {
        "sno": 80,
        "slokaNo": 65,
        "sloka": "यजतां पाण्डवः स्वर्गमवत्विन्द्र"
    },
    {
        "sno": 81,
        "slokaNo": 22,
        "sloka": "यद्वासुदेवेनाऽदीनमनादीनव"
    },
    {
        "sno": 82,
        "slokaNo": 92,
        "sloka": "यातव्यपार्ष्णिग्राहादिमाला"
    },
    {
        "sno": 83,
        "slokaNo": 13,
        "sloka": "यावदर्थपदां वाचमेवमादाय"
    },
    {
        "sno": 84,
        "slokaNo": 1,
        "sloka": "यियक्षमाणेनाहूतः पार्थेन"
    },
    {
        "sno": 85,
        "slokaNo": 98,
        "sloka": "ये चान्ये कालयवनशाल्वर"
    },
    {
        "sno": 86,
        "slokaNo": 4,
        "sloka": "रत्नस्तम्भेषु सङ्क्रान्तप्रतिमा"
    },
    {
        "sno": 87,
        "slokaNo": 58,
        "sloka": "लिलङ्घयिषतो लोकानलङ्घ्यान"
    },
    {
        "sno": 88,
        "slokaNo": 72,
        "sloka": "वर्णैः कतिपयैरेव ग्रथितस्य"
    },
    {
        "sno": 89,
        "slokaNo": 59,
        "sloka": "विजयस्त्वयि सेनायाः साक्षिमात्रे"
    },
    {
        "sno": 90,
        "slokaNo": 42,
        "sloka": "विधाय वैरं सामर्षे नरोऽरौ"
    },
    {
        "sno": 91,
        "slokaNo": 34,
        "sloka": "विपक्षमखिलीकृत्य प्रतिष्ठा"
    },
    {
        "sno": 92,
        "slokaNo": 41,
        "sloka": "विराद्ध एवं भवता"
    },
    {
        "sno": 93,
        "slokaNo": 25,
        "sloka": "विरोधिवचसो मूकान् वागी"
    },
    {
        "sno": 94,
        "slokaNo": 15,
        "sloka": "विवक्षितामर्थविदस्तत्क्षण"
    },
    {
        "sno": 95,
        "slokaNo": 75,
        "sloka": "विशेषविदुषः शास्त्रं यत्तवोद्ग्राह्यते-"
    },
    {
        "sno": 96,
        "slokaNo": 26,
        "sloka": "षड्गुणाः शक्तयस्तिस्रः सिद्धय"
    },
    {
        "sno": 97,
        "slokaNo": 93,
        "sloka": "षाड्गुण्यमुपयुञ्जीत शक्त्यपेक्षो"
    },
    {
        "sno": 98,
        "slokaNo": 36,
        "sloka": "सखा गरीयान् शत्रुश्च"
    },
    {
        "sno": 99,
        "slokaNo": 24,
        "sloka": "सङ्क्षिप्तस्याप्यतोऽस्यैव"
    },
    {
        "sno": 100,
        "slokaNo": 33,
        "sloka": "समूलघातमघ्नन्तः परान्नोद्यन्ति"
    },
    {
        "sno": 101,
        "slokaNo": 32,
        "sloka": " सम्पदा सुस्थिरंमन्यो"
    },
    {
        "sno": 102,
        "slokaNo": 97,
        "sloka": "सम्पादितफलस्तेन सपक्षः"
    },
    {
        "sno": 103,
        "slokaNo": 70,
        "sloka": "सम्प्रत्यसाम्प्रतं वक्तुमुक्ते"
    },
    {
        "sno": 104,
        "slokaNo": 103,
        "sloka": "सम्भाव्य त्वामतिभरक्षमस्कन्धं"
    },
    {
        "sno": 105,
        "slokaNo": 28,
        "sloka": "सर्वकार्यशरीरेषु मुक्त्वाङ्ग"
    },
    {
        "sno": 106,
        "slokaNo": 115,
        "sloka": "सविशेषं सुते पाण्डोर्भक्तिं"
    },
    {
        "sno": 107,
        "slokaNo": 117,
        "sloka": "सहजचापलदोषसमुद्धतश्चलित"
    },
    {
        "sno": 108,
        "slokaNo": 108,
        "sloka": "सहिष्ये शतमागांसि सूनोस्ते"
    },
    {
        "sno": 109,
        "slokaNo": 55,
        "sloka": "सामवादाः सकोपस्य"
    },
    {
        "sno": 110,
        "slokaNo": 2,
        "sloka": "सार्धमुद्धवसीरिभ्यामथासाव"
    },
    {
        "sno": 111,
        "slokaNo": 77,
        "sloka": "सोपधानां धियं धीराः स्थेयसीं"
    },
    {
        "sno": 112,
        "slokaNo": 78,
        "sloka": "स्पृशन्ति शरवत्तीक्ष्णाः स्तोक"
    },
    {
        "sno": 113,
        "slokaNo": 94,
        "sloka": "स्थाने शमवतां शक्त्या"
    },
    {
        "sno": 114,
        "slokaNo": 87,
        "sloka": "स्थायिनोऽर्थे प्रवर्तन्ते"
    },
    {
        "sno": 115,
        "slokaNo": 110,
        "sloka": "स्वयङ्कृतप्रसादस्य तस्याह्नो"
    },
    {
        "sno": 116,
        "slokaNo": 50,
        "sloka": "स्वयं प्रणमतेऽल्पेऽपि"
    },
    {
        "sno": 117,
        "slokaNo": 57,
        "sloka": "स्वशक्त्युपचये केचित्परस्य"
    },
    {
        "sno": 118,
        "slokaNo": 60,
        "sloka": "हते हिडिम्बरिपुणा राज्ञि"
    }
]