[
    {
        "sno": 1,
        "slokaNo": 15,
        "sloka": "अग्रे गतेन वसतिं परिगृह्य रम्याम् आपात्य"
    },
    {
        "sno": 2,
        "slokaNo": 43,
        "sloka": "अद्रीन्द्रकुञ्जचरकुञ्जरगण्डकाषसङ्क्रान्तदानपयसः"
    },
    {
        "sno": 3,
        "slokaNo": 38,
        "sloka": "अन्तर्जलौघम् अवगाढवतः कपोलौ हित्वा क्षणं"
    },
    {
        "sno": 4,
        "slokaNo": 25,
        "sloka": "अल्पप्रयोजनकृतोरुतरप्रयासैः उद्गूर्णलोष्टलगुडैः"
    },
    {
        "sno": 5,
        "slokaNo": 60,
        "sloka": "अव्याकुलं प्रकृतम् उत्तरधेयकर्म धाराः प्रसाधयितुम्"
    },
    {
        "sno": 6,
        "slokaNo": 58,
        "sloka": "अश्रावि भूमिपतिभिः क्षणवीतनिद्रैः अश्नन्"
    },
    {
        "sno": 7,
        "slokaNo": 32,
        "sloka": "आत्मानमेव जलधेः प्रतिबिम्बिताङ्गमूर्मौ"
    },
    {
        "sno": 8,
        "slokaNo": 6,
        "sloka": "आयस्तमैक्षत जनश्चटुलाग्रपादं गच्छन्त"
    },
    {
        "sno": 9,
        "slokaNo": 5,
        "sloka": "आरक्षमग्नमवमत्य सृणिं शिताग्रमेकः पलायत"
    },
    {
        "sno": 10,
        "slokaNo": 30,
        "sloka": "आलोलपुष्करमुखोल्लसितैरभीक्ष्णमुक्षाम्बभूवुरभितो"
    },
    {
        "sno": 11,
        "slokaNo": 4,
        "sloka": "आवर्तिनः शुभफलप्रदशुक्तियुक्ताः सम्पन्नदेवमणयः"
    },
    {
        "sno": 12,
        "slokaNo": 27,
        "sloka": "आस्तीर्णतल्परचितावसथः क्षणेन वेश्याजनः"
    },
    {
        "sno": 13,
        "slokaNo": 1,
        "sloka": "इत्थं गिरः प्रियतमा  इव सः अव्यलीकाः शुश्राव"
    },
    {
        "sno": 14,
        "slokaNo": 59,
        "sloka": "उत्खाय दर्पचलितेन सह एव रज्ज्वा कीलं"
    },
    {
        "sno": 15,
        "slokaNo": 12,
        "sloka": "उच्छिद्य  विद्विषः इव प्रसभं मृगेन्द्रान्"
    },
    {
        "sno": 16,
        "slokaNo": 22,
        "sloka": "उत्क्षिप्तकाण्डपटकान्तरलीयमानमन्दानिलप्रश"
    },
    {
        "sno": 17,
        "slokaNo": 62,
        "sloka": "उत्तीर्णभारलघुनाप्यलघूलपौघसौहित्यनिःसहतरेण"
    },
    {
        "sno": 18,
        "slokaNo": 9,
        "sloka": "उद्यत्कृशानुशकलेषु खुराभिघातात्"
    },
    {
        "sno": 19,
        "slokaNo": 68,
        "sloka": "उन्नम्रताम्रपटमण्डपमण्डितं तत् आनीलनाग"
    },
    {
        "sno": 20,
        "slokaNo": 45,
        "sloka": "उष्णोष्णशीकरसृजः प्रबलोष्मणः अन्तः"
    },
    {
        "sno": 21,
        "slokaNo": 18,
        "sloka": "कण्ठावसक्तमृदुबाहुलताः तुरङ्गात् राजावरोधन"
    },
    {
        "sno": 22,
        "slokaNo": 46,
        "sloka": "कण्डूयतः कटभुवं करिणः मदेन स्कन्धं"
    },
    {
        "sno": 23,
        "slokaNo": 35,
        "sloka": "कीर्णं शनैरनुकपोलमनेकपानां हस्तैर्विगाढमदतापरुजः"
    },
    {
        "sno": 24,
        "slokaNo": 50,
        "sloka": "क्षिप्तम् पुरः न जगृहे मुहुः इक्षुकाण्डं न अपेक्षते"
    },
    {
        "sno": 25,
        "slokaNo": 36,
        "sloka": "गण्डूषम् उज्झितवता पयसः सरोषं नागेन"
    },
    {
        "sno": 26,
        "slokaNo": 53,
        "sloka": "गत्यूनमार्गगतयोऽपि गतोरुमार्गाः स्वैरं"
    },
    {
        "sno": 27,
        "slokaNo": 14,
        "sloka": "छायामपास्य महतीमपि वर्तमानामागामिनीं"
    },
    {
        "sno": 28,
        "slokaNo": 21,
        "sloka": "छायाविधायिभिरनुज्झितभूतिशोभैरुच्छ्रायिभिर्बहल"
    },
    {
        "sno": 29,
        "slokaNo": 49,
        "sloka": "जज्ञे जनैर्मुकुलिताक्षमनाददाने संरब्धहस्तिपक"
    },
    {
        "sno": 30,
        "slokaNo": 2,
        "sloka": "तं सः द्विपेन्द्रतुलितातुलतुङ्गश्रृङ्गमभ्युल्लसत्"
    },
    {
        "sno": 31,
        "slokaNo": 10,
        "sloka": "तेजोनिरोधसमतावहितेन यन्त्रा सम्यक्कशात्रय"
    },
    {
        "sno": 32,
        "slokaNo": 7,
        "sloka": "त्रस्तः समस्तजनहासकरः करेणोस्तावत्खरः"
    },
    {
        "sno": 33,
        "slokaNo": 26,
        "sloka": "त्रासाकुलः परिपतन् परितः निकेतान् पुम्भिः न"
    },
    {
        "sno": 34,
        "slokaNo": 56,
        "sloka": "दन्तालिकाधरणनिश्चलपाणियुग्ममर्धोदितः"
    },
    {
        "sno": 35,
        "slokaNo": 37,
        "sloka": "दानं ददत्यपि जलैः सहसाधिरूढे को विद्यमान"
    },
    {
        "sno": 36,
        "slokaNo": 51,
        "sloka": "दुःखेन भोजयितुमाशयिता शशाक तुङ्गाग्रकायम्"
    },
    {
        "sno": 37,
        "slokaNo": 19,
        "sloka": "दृष्ट्वैव निर्जितकलापभरामधस्ताद् व्याकीर्णमाल्य"
    },
    {
        "sno": 38,
        "slokaNo": 69,
        "sloka": "धरस्योद्धर्तासि त्वमिति ननु सर्वत्र जगति"
    },
    {
        "sno": 39,
        "slokaNo": 33,
        "sloka": "नादातुमन्यकरिमुक्तमदाम्बुतिक्तं धूताङ्कुशेन न"
    },
    {
        "sno": 40,
        "slokaNo": 44,
        "sloka": "नोच्चैर्यदा तरुतलेषु ममुस्तदानीमाधोरणैरभिहिताः"
    },
    {
        "sno": 41,
        "slokaNo": 29,
        "sloka": "नाभिह्रदैः परिगृहीतरयाणि निम्नैः स्त्रीणां"
    },
    {
        "sno": 42,
        "slokaNo": 47,
        "sloka": "निर्धूतवीतमपि बालकमुल्ललन्तं यन्ता क्रमेण"
    },
    {
        "sno": 43,
        "slokaNo": 11,
        "sloka": "नीहारजालमलिनः पुनरुक्तसान्द्राः कुर्वन्"
    },
    {
        "sno": 44,
        "slokaNo": 34,
        "sloka": "पन्थानमाशु विजहीहि पुरः स्तनौ ते पश्यन्"
    },
    {
        "sno": 45,
        "slokaNo": 41,
        "sloka": "प्रत्यन्यदन्ति निशिताङ्कुशदूरभिन्ननिर्याण"
    },
    {
        "sno": 46,
        "slokaNo": 23,
        "sloka": "प्रस्वेदवारिसविशेषविषक्तमङ्गे कूर्पासकं"
    },
    {
        "sno": 47,
        "slokaNo": 65,
        "sloka": "बिभ्राणमायतिमतीमवृथा शिरोधिं प्रत्यग्रतामति"
    },
    {
        "sno": 48,
        "slokaNo": 13,
        "sloka": "बिभ्राणया बहलयावकपङ्कपिङ्गपिच्छावचूडम्"
    },
    {
        "sno": 49,
        "slokaNo": 3,
        "sloka": "भास्वत्करव्यतिकरोल्लसिताम्बरान्ताः सापत्रपाः "
    },
    {
        "sno": 50,
        "slokaNo": 61,
        "sloka": "मुक्ताः तृणानि परितः कटकं चरन्तः"
    },
    {
        "sno": 51,
        "slokaNo": 64,
        "sloka": "मेदस्विनः सरभसोपगतान् अभीकान् भङ्क्त्वा"
    },
    {
        "sno": 52,
        "slokaNo": 63,
        "sloka": "मृत्पिण्डशेखरितकोटिभिः अर्धचन्द्रं श्रृङ्गैः"
    },
    {
        "sno": 53,
        "slokaNo": 17,
        "sloka": "यानात् जनः परिजनैः अवरोप्यमाणाः राज्ञीः"
    },
    {
        "sno": 54,
        "slokaNo": 40,
        "sloka": "यां चन्द्रकैः मदजलस्य महानदीनां नेत्रश्रियं"
    },
    {
        "sno": 55,
        "slokaNo": 24,
        "sloka": "यावत् स एव समयः समम् एव तावत्"
    },
    {
        "sno": 56,
        "slokaNo": 31,
        "sloka": "ये पक्षिणः प्रथममम्बुनिधिं गताः ते ये अपि"
    },
    {
        "sno": 57,
        "slokaNo": 57,
        "sloka": "रेजे जनैः स्नपनसान्द्रतरार्द्रमूर्तिः देवैरिव"
    },
    {
        "sno": 58,
        "slokaNo": 20,
        "sloka": "रोचिष्णुकाञ्चनचयांशुपिशङ्गिताशाः वंशध्वजैः"
    },
    {
        "sno": 59,
        "slokaNo": 54,
        "sloka": "व्याजिघ्रति प्रणतमूर्धनि बाह्लिजेऽश्वे तस्याङ्गसङ्ग"
    },
    {
        "sno": 60,
        "slokaNo": 52,
        "sloka": "शुक्लांशुकोपरचितानि निरन्तराभिः वेश्मानि"
    },
    {
        "sno": 61,
        "slokaNo": 8,
        "sloka": "शैलोपशल्यनिपतद्रथनेमिधारानिष्पिष्टनिष्ठुर"
    },
    {
        "sno": 62,
        "slokaNo": 28,
        "sloka": "सस्नुः पयः पपुरनेनिजुरम्बराणि जक्षुर्बिसं"
    },
    {
        "sno": 63,
        "slokaNo": 66,
        "sloka": "सार्धं कथञ्चिदुचितैः पिचुमर्दपत्रैरास्यन्तरालगत"
    },
    {
        "sno": 64,
        "slokaNo": 16,
        "sloka": "सिक्ता इवामृतरसेन मुहुर्जनानां क्लान्तिच्छिदो"
    },
    {
        "sno": 65,
        "slokaNo": 42,
        "sloka": "सेव्योऽपि सानुनयमाकलनाय यन्त्रा नीतेन"
    },
    {
        "sno": 66,
        "slokaNo": 39,
        "sloka": "संसर्पिभिः पयसि गैरिकरेणुरागैरम्भोजगर्भरजसा"
    },
    {
        "sno": 67,
        "slokaNo": 48,
        "sloka": "स्तम्भं महान्तम् उचितं सहसा मुमोच दानं ददौ"
    },
    {
        "sno": 68,
        "slokaNo": 67,
        "sloka": "स्पष्टं बहिः स्थितवतेऽपि निवेदयन्तश्चेष्टाविशेष"
    },
    {
        "sno": 69,
        "slokaNo": 55,
        "sloka": "हेम्नः स्थलीषु परितः परिवृत्य वाजी धुन्वन् वपुः"
    }
]