[
    {
        "sno": 1,
        "slokaNo": 66,
        "sloka": "अतनुकुचभरानतेन भूयः श्रमजनिता"
    },
    {
        "sno": 2,
        "slokaNo": 5,
        "sloka": "अतिशयपरिणाहवान् वितेने बहुतरमर्पित"
    },
    {
        "sno": 3,
        "slokaNo": 36,
        "sloka": "अथ किल कथिते सखीभिरत्र क्षणमपरेव"
    },
    {
        "sno": 4,
        "slokaNo": 63,
        "sloka": "अधिकमरुणिमानमुद्वहद्भिर्विकस"
    },
    {
        "sno": 5,
        "slokaNo": 52,
        "sloka": "अधिरजनि जगाम धाम तस्याः प्रियतम"
    },
    {
        "sno": 6,
        "slokaNo": 31,
        "sloka": "अनवरतरसेन रागभाजा करजपरिक्षति"
    },
    {
        "sno": 7,
        "slokaNo": 1,
        "sloka": "अनुगिरमृतुभिर्वितायमानामथ स विलोकयितुं"
    },
    {
        "sno": 8,
        "slokaNo": 22,
        "sloka": "अनुवनमसितभ्रुवः सखीभिः सह"
    },
    {
        "sno": 9,
        "slokaNo": 21,
        "sloka": "अनुवपुरपरेण बाहुमूलप्रहितभुजा"
    },
    {
        "sno": 10,
        "slokaNo": 67,
        "sloka": "अपगतनवयावकैश्चिराय क्षितिगमनेन"
    },
    {
        "sno": 11,
        "slokaNo": 72,
        "sloka": "अभिमतमभितः कृताङ्गभङ्गा"
    },
    {
        "sno": 12,
        "slokaNo": 29,
        "sloka": "अभिमुखपतितैर्गुणप्रकर्षादवजितमुद्धति"
    },
    {
        "sno": 13,
        "slokaNo": 41,
        "sloka": "अभिमुखमुपयाति मा स्म किञ्चित्वमभिदधाः"
    },
    {
        "sno": 14,
        "slokaNo": 61,
        "sloka": "अवचितकुसुमा विहाय वल्लीर्युवतिषु"
    },
    {
        "sno": 15,
        "slokaNo": 60,
        "sloka": "अवजितमधुना तवाऽहमक्ष्णो रुचिर"
    },
    {
        "sno": 16,
        "slokaNo": 38,
        "sloka": "अवनतवदनेन्दुरिच्छतीव व्यवधिमधीरतया"
    },
    {
        "sno": 17,
        "slokaNo": 3,
        "sloka": "अवसरमधिगम्य तं हरन्त्यो हृदयमयत्न"
    },
    {
        "sno": 18,
        "slokaNo": 64,
        "sloka": "अवसितललितक्रियेण बाह्वोर्ललिततरेण"
    },
    {
        "sno": 19,
        "slokaNo": 71,
        "sloka": "अविरतकुसुमावचायखेदान्निहितभुजा"
    },
    {
        "sno": 20,
        "slokaNo": 15,
        "sloka": "अविरलपुलकः सह व्रजन्त्याः प्रतिपदमेकतरः"
    },
    {
        "sno": 21,
        "slokaNo": 16,
        "sloka": "अशिथिलमपराऽवसज्य कण्ठे दृढपरि"
    },
    {
        "sno": 22,
        "slokaNo": 26,
        "sloka": "असकलकलिकाऽऽकुलीकृतालिस्खलनविकीर्ण"
    },
    {
        "sno": 23,
        "slokaNo": 56,
        "sloka": "इति गदितवती रुषा जघान"
    },
    {
        "sno": 24,
        "slokaNo": 13,
        "sloka": "इति वदति सखीजनेऽनुरागाद्दयिततमामपरश्चिरं"
    },
    {
        "sno": 25,
        "slokaNo": 43,
        "sloka": "इति वदति सखीजने निमीलद्द्विगुणित"
    },
    {
        "sno": 26,
        "slokaNo": 50,
        "sloka": "इदमिदमिति भूरुहां प्रसूनैर्मुहुरतिलोभ"
    },
    {
        "sno": 27,
        "slokaNo": 49,
        "sloka": "उपरिजतरुजानि याचमानां कुशलतया"
    },
    {
        "sno": 28,
        "slokaNo": 27,
        "sloka": "उपवनपवनानुपातदक्षैरलिभिरलाभि "
    },
    {
        "sno": 29,
        "slokaNo": 39,
        "sloka": "किसलयशकलेष्ववाचनीयाः पुलकिनि"
    },
    {
        "sno": 30,
        "slokaNo": 40,
        "sloka": "कृतकृतकरुष सखीमपास्य त्वमकुशलेति"
    },
    {
        "sno": 31,
        "slokaNo": 37,
        "sloka": "कृतभयपरितोषसन्निपातं सचकितसस्मित"
    },
    {
        "sno": 32,
        "slokaNo": 10,
        "sloka": "गतधृतिरवलम्बितुं बताऽसूननलमनालपनादहं"
    },
    {
        "sno": 33,
        "slokaNo": 74,
        "sloka": "गत्वोद्रेकं जघनपुलिने रुद्धमध्यप्रदेशः क्रामन्नूरुद्रुम"
    },
    {
        "sno": 34,
        "slokaNo": 18,
        "sloka": "गुरुतरकलनूपुरानुनादं सललितनर्तित"
    },
    {
        "sno": 35,
        "slokaNo": 6,
        "sloka": "गुरुनिबिडनितम्बबिम्बभाराक्रमणनिपीडित"
    },
    {
        "sno": 36,
        "slokaNo": 20,
        "sloka": "जघनमलघुपीवरोरु कृच्छ्रादुरुनिबिरीस"
    },
    {
        "sno": 37,
        "slokaNo": 54,
        "sloka": "तव कितव! किमाहितैर्वृथा नः क्षितिरुह"
    },
    {
        "sno": 38,
        "slokaNo": 7,
        "sloka": "तव सपदि समीपमानये तामहमिति तस्य"
    },
    {
        "sno": 39,
        "slokaNo": 2,
        "sloka": "दधति सुमनसो वनानि बह्वीर्युवतियुता"
    },
    {
        "sno": 40,
        "slokaNo": 12,
        "sloka": "द्रुतपदमिति मा वयस्य यासीर्ननु सुतनुं"
    },
    {
        "sno": 41,
        "slokaNo": 4,
        "sloka": "नखरुचिरचितेन्द्रचापलेखं ललितगतेषु"
    },
    {
        "sno": 42,
        "slokaNo": 53,
        "sloka": "न खलु वयममुष्य दानयोग्याः पिबति च"
    },
    {
        "sno": 43,
        "slokaNo": 8,
        "sloka": "न च सुतनु! न वेद्मि यन्महीयानसुनिरसस्तव"
    },
    {
        "sno": 44,
        "slokaNo": 69,
        "sloka": "प्रथममलघुमौक्तिकाभमासीच्छ्रमजल"
    },
    {
        "sno": 45,
        "slokaNo": 34,
        "sloka": "प्रसकलकुचबन्धुरोद्धुरोरःप्रसभविभिन्न"
    },
    {
        "sno": 46,
        "slokaNo": 75,
        "sloka": "प्रियकरपरिमार्गादङ्गनानां यदाऽभूत्"
    },
    {
        "sno": 47,
        "slokaNo": 32,
        "sloka": "प्रियमभि कुसुमोद्यतस्य बाहोर्नवनख"
    },
    {
        "sno": 48,
        "slokaNo": 11,
        "sloka": "प्रियमिति वनिता नितान्तमागःस्मरण"
    },
    {
        "sno": 49,
        "slokaNo": 23,
        "sloka": "मदनरसमहौघपूर्णनाभीह्रदपरिवाहित"
    },
    {
        "sno": 50,
        "slokaNo": 25,
        "sloka": "मधुमथनवधूरिवाह्वयन्ति भ्रमरकुलानि"
    },
    {
        "sno": 51,
        "slokaNo": 44,
        "sloka": "मुखकमलकमुन्नमय्य यूना यदभिनवो"
    },
    {
        "sno": 52,
        "slokaNo": 30,
        "sloka": "मुदितमधुभुजो भुजेन शाखाश्चलितविश्रृङ्ख"
    },
    {
        "sno": 53,
        "slokaNo": 17,
        "sloka": "मुहुरसुममाघ्नती नितान्तं प्रणदित"
    },
    {
        "sno": 54,
        "slokaNo": 68,
        "sloka": "मुहुरिति वनविभ्रमाऽभिषङ्गादतमि तदा"
    },
    {
        "sno": 55,
        "slokaNo": 55,
        "sloka": "मुहुरुपहसितामिवालिनादैर्वितरसि नः"
    },
    {
        "sno": 56,
        "slokaNo": 48,
        "sloka": "मृदुचरणतलाग्रदुःस्थितत्वादसहतरा"
    },
    {
        "sno": 57,
        "slokaNo": 14,
        "sloka": "यदि मयि लघिमानमागतायां तव धृतिरस्ति"
    },
    {
        "sno": 58,
        "slokaNo": 28,
        "sloka": "रथचरणधराङ्गनाकराब्जव्यतिकर"
    },
    {
        "sno": 59,
        "slokaNo": 19,
        "sloka": "लघुललितपदं तदंसपीठद्वयनिहितोभयपाणि"
    },
    {
        "sno": 60,
        "slokaNo": 51,
        "sloka": " विजनमिति बलादमुं गृहीत्वा क्षणमथ"
    },
    {
        "sno": 61,
        "slokaNo": 33,
        "sloka": "विततवलिविभाव्यपाण्डुलेखाकृतपरभाग"
    },
    {
        "sno": 62,
        "slokaNo": 57,
        "sloka": "विनयति सुदृशो दृशः परागं प्रणयिनि"
    },
    {
        "sno": 63,
        "slokaNo": 70,
        "sloka": "विपुलकमपि यौवनोद्धतानां घनपुलकोदय"
    },
    {
        "sno": 64,
        "slokaNo": 46,
        "sloka": "विलसितमनुकुर्वती पुरस्ताद्धरणिरुहाधिरुहो"
    },
    {
        "sno": 65,
        "slokaNo": 35,
        "sloka": "व्यवहितमविजानती किलाऽन्तर्वणभुवि"
    },
    {
        "sno": 66,
        "slokaNo": 45,
        "sloka": "व्रततिविततिभिस्तिरोहितायां प्रतियुवतौ"
    },
    {
        "sno": 67,
        "slokaNo": 24,
        "sloka": "श्रुतिपथमधुराणि सारसानामनुनदि शुश्रुविरे"
    },
    {
        "sno": 68,
        "slokaNo": 62,
        "sloka": " श्लथशिरसिजपाशपातभारादिव नितरां"
    },
    {
        "sno": 69,
        "slokaNo": 9,
        "sloka": "सततमनभिभाषणं मया ते परिपणितं"
    },
    {
        "sno": 70,
        "slokaNo": 59,
        "sloka": "समदनमवतंसितेऽधिकर्णं प्रणयवता"
    },
    {
        "sno": 71,
        "slokaNo": 42,
        "sloka": " सरजसमकरन्दनिर्भरासु प्रसवविभूतिषु"
    },
    {
        "sno": 72,
        "slokaNo": 47,
        "sloka": "सललितमवलम्ब्य पाणिनांसे सहचर"
    },
    {
        "sno": 73,
        "slokaNo": 58,
        "sloka": "स्फुटमिदमभिचारमन्त्र एव प्रतियुवतेरभिधान"
    },
    {
        "sno": 74,
        "slokaNo": 65,
        "sloka": "स्मरसरसमुरःस्थलेन पत्युर्विनिमय"
    },
    {
        "sno": 75,
        "slokaNo": 73,
        "sloka": " हिमलवसदृशः श्रमोदबिन्दूनपनयता किल"
    }
]