[
    {
        "sno": 1,
        "slokaNo": 15,
        "sloka": "अजगणन् गणशः प्रियमग्रतः प्रणतमप्यभिमानि"
    },
    {
        "sno": 2,
        "slokaNo": 67,
        "sloka": "अतिसुरभिरभाजि पुष्पश्रियामतनुतरतयेव"
    },
    {
        "sno": 3,
        "slokaNo": 1,
        "sloka": "अथ रिरंसुममुं युगपद् गिरौ कृतयथास्वतरु"
    },
    {
        "sno": 4,
        "slokaNo": 66,
        "sloka": "अधिलवङ्गममी रजसाधिकं मलिनिता सुमनोदल"
    },
    {
        "sno": 5,
        "slokaNo": 27,
        "sloka": "अनुययौ विविधोपलकुण्डलद्युतिवितानक"
    },
    {
        "sno": 6,
        "slokaNo": 46,
        "sloka": "अनुवनं वनराजिवधूमुखे बहलराग"
    },
    {
        "sno": 7,
        "slokaNo": 64,
        "sloka": "अभिषिषेणयिषुं भुवनानि यः स्मरमिवाख्यत"
    },
    {
        "sno": 8,
        "slokaNo": 74,
        "sloka": "अभीष्टमासाद्य चिराय काले समुद्धृताशं"
    },
    {
        "sno": 9,
        "slokaNo": 40,
        "sloka": "अरमयन् भवनादचिरद्युतेः किल"
    },
    {
        "sno": 10,
        "slokaNo": 21,
        "sloka": "अरुणिताखिलशैलवना मुहुर्विदधती"
    },
    {
        "sno": 11,
        "slokaNo": 13,
        "sloka": "इति गदन्तमनन्तरमङ्गना भुजयुगोन्नमनो"
    },
    {
        "sno": 12,
        "slokaNo": 11,
        "sloka": "इदमपास्य विरागि परागिणीरलिकदम्बक"
    },
    {
        "sno": 13,
        "slokaNo": 56,
        "sloka": "इदमयुक्तमहो महदेव यद् वरतनोः स्मर"
    },
    {
        "sno": 14,
        "slokaNo": 63,
        "sloka": "उपचितेषु परेष्वसमर्थतां व्रजति कालवशाद्"
    },
    {
        "sno": 15,
        "slokaNo": 47,
        "sloka": "कनकभङ्गपिशङ्गदलैर्दधे सरजसारुणकेसर"
    },
    {
        "sno": 16,
        "slokaNo": 77,
        "sloka": "कान्ताजनेन रहसि प्रसभं गृहीतकेशे रते"
    },
    {
        "sno": 17,
        "slokaNo": 73,
        "sloka": "कुटजानि वीक्ष्य शिखिभिः शिखरीन्द्रं"
    },
    {
        "sno": 18,
        "slokaNo": 79,
        "sloka": "कुर्वन्तमित्यतिभरेण नगानवाचः पुष्पैर्विराम"
    },
    {
        "sno": 19,
        "slokaNo": 16,
        "sloka": "कुसुमकार्मुककार्मुकसंहितद्रुतशिलीमुख"
    },
    {
        "sno": 20,
        "slokaNo": 62,
        "sloka": "कुसुमयन्फलिनीरलिनीरवैर्मदविकासि"
    },
    {
        "sno": 21,
        "slokaNo": 50,
        "sloka": "कृतमदं निगदन्त इवाकुलीकृतजगत्त्रय"
    },
    {
        "sno": 22,
        "slokaNo": 26,
        "sloka": "गजकदम्बकमेचकमुच्चकैर्नभसि वीक्ष्य"
    },
    {
        "sno": 23,
        "slokaNo": 55,
        "sloka": "गजपतिद्वयसीरपि हैमनः तुहिनयन् सरितः"
    },
    {
        "sno": 24,
        "slokaNo": 78,
        "sloka": "गतवतामिव विस्मयमुच्चकैरसकलामलपल्लव"
    },
    {
        "sno": 25,
        "slokaNo": 43,
        "sloka": "जगति नैशमशीतकरः करैर्वियति वारि"
    },
    {
        "sno": 26,
        "slokaNo": 69,
        "sloka": "जगद्वशीकर्तुमिमाः स्मरस्य प्रभावनीके तनवै"
    },
    {
        "sno": 27,
        "slokaNo": 31,
        "sloka": "जलदपङ्क्तिरनर्तयदुन्मदं कलविलापि"
    },
    {
        "sno": 28,
        "slokaNo": 76,
        "sloka": "जातप्रीतिर्या मधुरेणानुवनान्तं कामो कान्ते"
    },
    {
        "sno": 29,
        "slokaNo": 29,
        "sloka": "पटलमम्बुमुचां पथिकाङ्गना सपदि जीवित"
    },
    {
        "sno": 30,
        "slokaNo": 38,
        "sloka": "प्रणयकोपभृतोऽपि पराङ्मुखाः सपदि"
    },
    {
        "sno": 31,
        "slokaNo": 30,
        "sloka": "प्रवसतः सुतरामुदकम्पयद् विदलकन्दल"
    },
    {
        "sno": 32,
        "slokaNo": 57,
        "sloka": "प्रियतमेन यया सरुषा स्थितं न सह सा"
    },
    {
        "sno": 33,
        "slokaNo": 8,
        "sloka": "प्रियसखीसदृशं प्रतिबोधिताः किमपि"
    },
    {
        "sno": 34,
        "slokaNo": 58,
        "sloka": "भृशमदूयत याऽधरपल्लवक्षतिरनावरणा"
    },
    {
        "sno": 35,
        "slokaNo": 9,
        "sloka": "मधुकरैरपवादकरैरिव स्मृतिभुवः पथिका"
    },
    {
        "sno": 36,
        "slokaNo": 20,
        "sloka": "मधुरया मधुबोधितमाधवीमधुसमृद्धि"
    },
    {
        "sno": 37,
        "slokaNo": 48,
        "sloka": "मुखसरोजरुचं मदपाटलामनुचकार चकोर"
    },
    {
        "sno": 38,
        "slokaNo": 72,
        "sloka": "मुदमब्दभुवामपां मयूराः सहसायन्त"
    },
    {
        "sno": 39,
        "slokaNo": 45,
        "sloka": "तनुरुहाणि पुरो विजितध्वनेर्धवलपक्षविहङ्ग"
    },
    {
        "sno": 40,
        "slokaNo": 4,
        "sloka": "तुलयति स्म विलोचनतारकाः कुरबकस्तबक"
    },
    {
        "sno": 41,
        "slokaNo": 18,
        "sloka": "त्यजति कष्टमसावचिरादसून् विरहवेदन"
    },
    {
        "sno": 42,
        "slokaNo": 41,
        "sloka": "ददतमन्तरिताऽहिमदीधितिं खगकुलाय"
    },
    {
        "sno": 43,
        "slokaNo": 23,
        "sloka": "दलितकोमलपाटलकुड्मले निजवधूश्वसिता"
    },
    {
        "sno": 44,
        "slokaNo": 35,
        "sloka": "दलितमौक्तिकचूर्णविपाण्डवः स्फुरितनिर्झर"
    },
    {
        "sno": 45,
        "slokaNo": 28,
        "sloka": "द्रुतसमीरचलैः क्षणलक्षितव्यवहिता विटपैरिव"
    },
    {
        "sno": 46,
        "slokaNo": 34,
        "sloka": "द्विरददन्तवलक्षमलक्ष्यत स्फुरितभृङ्गमृग"
    },
    {
        "sno": 47,
        "slokaNo": 60,
        "sloka": "धृततुषारकणस्य नभस्वतस्तरुलताङ्गुलित"
    },
    {
        "sno": 48,
        "slokaNo": 19,
        "sloka": "न खलु दूरगतोऽप्यतिवर्तते महमसाविति"
    },
    {
        "sno": 49,
        "slokaNo": 32,
        "sloka": "नवकदम्बरजोऽरुणिताम्बरैरधिपुरन्ध्रि"
    },
    {
        "sno": 50,
        "slokaNo": 36,
        "sloka": "नवपयःकणकोमलमालतीकुसुमसन्तति"
    },
    {
        "sno": 51,
        "slokaNo": 2,
        "sloka": " नवपलाशपलाशवनं पुरः स्फुटपराग"
    },
    {
        "sno": 52,
        "slokaNo": 37,
        "sloka": "निजरजः पटवासमिवाऽकिरद् धृतपटोपम"
    },
    {
        "sno": 53,
        "slokaNo": 24,
        "sloka": " निदधिरे दयितोरसि तत्क्षणस्नपनवारि"
    },
    {
        "sno": 54,
        "slokaNo": 68,
        "sloka": "नोज्झितुं युवतिमाननिरासे दक्षमिष्टमधुवास"
    },
    {
        "sno": 55,
        "slokaNo": 7,
        "sloka": "रतिपतिप्रहितेव कृतक्रुधः प्रियतमेषु"
    },
    {
        "sno": 56,
        "slokaNo": 22,
        "sloka": "रवितुरङ्गतनूरुहतुल्यतां दधति यत्र"
    },
    {
        "sno": 57,
        "slokaNo": 17,
        "sloka": "रुरुदिषा वदनाम्बुरुहश्रियः सुतनु"
    },
    {
        "sno": 58,
        "slokaNo": 14,
        "sloka": "वदनसौरभलोभपरिभ्रमद्भ्रमरसम्भ्रम"
    },
    {
        "sno": 59,
        "slokaNo": 71,
        "sloka": "वपुरम्बुविहारहिमं शुचिना रुचिरं कमनीयतरा"
    },
    {
        "sno": 60,
        "slokaNo": 39,
        "sloka": "विगतरागगुणोऽपि जनो न कश्चलति वाति"
    },
    {
        "sno": 61,
        "slokaNo": 51,
        "sloka": "विगतवारिधरावरणाः क्वचिद् ददृशुरुल्लसिता"
    },
    {
        "sno": 62,
        "slokaNo": 49,
        "sloka": "विगतसस्यजिघत्समघट्टयत्कलमगोपवधूर्न"
    },
    {
        "sno": 63,
        "slokaNo": 52,
        "sloka": "विलुलितामनिलैः शरदङ्गना नवसरोरुहकेसर"
    },
    {
        "sno": 64,
        "slokaNo": 3,
        "sloka": "विलुलितालकसंहतिरामृशन् मृगदृशां"
    },
    {
        "sno": 65,
        "slokaNo": 59,
        "sloka": "व्रणभृता सुतनोः कलसीत्कृतस्फुरितदन्त"
    },
    {
        "sno": 66,
        "slokaNo": 33,
        "sloka": "शमिततापमपोढमहीरजः प्रथमबिन्दु"
    },
    {
        "sno": 67,
        "slokaNo": 67,
        "sloka": "शिशिरमासमपास्य गुणोऽस्य नः क इव"
    },
    {
        "sno": 68,
        "slokaNo": 10,
        "sloka": " समभिसृत्य रसादवलम्बितः प्रमदया"
    },
    {
        "sno": 69,
        "slokaNo": 44,
        "sloka": "समय एव करोति बलाबलं प्रणिवदन्त"
    },
    {
        "sno": 70,
        "slokaNo": 42,
        "sloka": "स विकचोत्पलचक्षुषमैक्षत क्षितिभृतोऽङ्कगतां"
    },
    {
        "sno": 71,
        "slokaNo": 12,
        "sloka": "सुरभिणि श्वसिते दधतस्तृषं नवसुधामधुरे"
    },
    {
        "sno": 72,
        "slokaNo": 75,
        "sloka": "स्तनयोः समयेन याऽङ्गनानामभिनद्धारसमा"
    },
    {
        "sno": 73,
        "slokaNo": 5,
        "sloka": "स्फुटमिवोज्ज्वलकाश्चनकान्तिभिर्युतमशोक"
    },
    {
        "sno": 74,
        "slokaNo": 25,
        "sloka": "स्फुरदधीरतडिन्नयना मुहुः प्रियमिवागलितोरु "
    },
    {
        "sno": 75,
        "slokaNo": 70,
        "sloka": "स्मररागमयी वपुस्तमिस्रा परितस्तार"
    },
    {
        "sno": 76,
        "slokaNo": 6,
        "sloka": "स्मरहुताशनमुर्मुरचूर्णतां दधुरिवाम्रवनस्य"
    },
    {
        "sno": 77,
        "slokaNo": 54,
        "sloka": "स्मितसरोरुहनेत्रसरोजलामतिसिताङ्गविहङ्ग"
    },
    {
        "sno": 78,
        "slokaNo": 53,
        "sloka": "हरितपत्रमयीव मरुद्गणैः स्रगवनद्धमनोरम"
    },
    {
        "sno": 79,
        "slokaNo": 61,
        "sloka": "हिमऋतावपि ताः स्म भृशस्विदो युवतयः"
    }
]