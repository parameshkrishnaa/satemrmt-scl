[
    {
        "sno": 1,
        "slokaNo": 31,
        "sloka": "अथ लक्ष्मणानुगतकान्तवपुर्जलधिं विलङ्घ्य"
    },
    {
        "sno": 2,
        "slokaNo": 15,
        "sloka": "अथ सान्द्रसान्ध्यकिरणाऽरुणितं हरिहेतिहूति"
    },
    {
        "sno": 3,
        "slokaNo": 46,
        "sloka": "अधरेष्वलक्तकरसः सुदृशां विशदं कपोलभुवि"
    },
    {
        "sno": 4,
        "slokaNo": 54,
        "sloka": "अधिजानु बाहुमुपधाय नमत्करपल्लवार्पित"
    },
    {
        "sno": 5,
        "slokaNo": 73,
        "sloka": "अनुदेहमागतवतः प्रतिमां परिणायकस्य"
    },
    {
        "sno": 6,
        "slokaNo": 10,
        "sloka": "अनुरागवन्तमपि लोचनयोर्दधतं वपुः"
    },
    {
        "sno": 7,
        "slokaNo": 24,
        "sloka": "अनुलेपनानि कुसुमान्यबलाः कृतमन्यवः"
    },
    {
        "sno": 8,
        "slokaNo": 83,
        "sloka": "अपयाति सरोषया निरस्ते कृतकं कामिनि"
    },
    {
        "sno": 9,
        "slokaNo": 4,
        "sloka": "अपराह्णशीतलतरेण शनैरनिलेन"
    },
    {
        "sno": 10,
        "slokaNo": 1,
        "sloka": "अभितापसम्पदमथोष्णरुचिर्निजतेज"
    },
    {
        "sno": 11,
        "slokaNo": 11,
        "sloka": "अभितिग्मरश्मि चिरमाविरमादवधानखिन्न"
    },
    {
        "sno": 12,
        "slokaNo": 37,
        "sloka": "अमलात्मसु प्रतिफलन्नभितस्तरुणीकपोलकेषु"
    },
    {
        "sno": 13,
        "slokaNo": 36,
        "sloka": "अमृतद्रवैर्विदधदब्जदृशाममपमार्गमोषधिपतिः"
    },
    {
        "sno": 14,
        "slokaNo": 22,
        "sloka": "अवधार्य कार्यगुरुतामभवन्न भयाय सान्द्रतम"
    },
    {
        "sno": 15,
        "slokaNo": 59,
        "sloka": "अवधीर्य धैर्यकलिता दयितं विदधे विरोधमथ"
    },
    {
        "sno": 16,
        "slokaNo": 74,
        "sloka": "अवनम्य वक्षसि निमग्नकुचद्वितयेन"
    },
    {
        "sno": 17,
        "slokaNo": 40,
        "sloka": "अविभावितेषुविषयः प्रथमं मदनोऽपि"
    },
    {
        "sno": 18,
        "slokaNo": 12,
        "sloka": "अविभाव्यतारकमदृष्टहिमद्युतिबिम्ब"
    },
    {
        "sno": 19,
        "slokaNo": 84,
        "sloka": "आलोक्य प्रियतममंशुके विनीवौ यत्तस्थे"
    },
    {
        "sno": 20,
        "slokaNo": 82,
        "sloka": " इति गन्तुमिच्छुमभिधाय पुरः क्षणदृष्टिपात"
    },
    {
        "sno": 21,
        "slokaNo": 43,
        "sloka": "इति निश्चितप्रियतमागतयः सितदीधितावुदय"
    },
    {
        "sno": 22,
        "slokaNo": 87,
        "sloka": "इत्थं नारीर्घटयितुमलं कामिभिः काममासन्"
    },
    {
        "sno": 23,
        "slokaNo": 30,
        "sloka": "उदमज्जि कैटभजितः शयनादपनिद्रपाण्डुरसरोज"
    },
    {
        "sno": 24,
        "slokaNo": 77,
        "sloka": "उदितोरुसादमतिवेपथुमत्सुदृशोऽभिभर्तृ"
    },
    {
        "sno": 25,
        "slokaNo": 69,
        "sloka": "उदितं प्रियां प्रति सहार्दमिति श्रदधीयत"
    },
    {
        "sno": 26,
        "slokaNo": 38,
        "sloka": "उपगूढवेलमलघूर्मिभुजैः सरितामचुक्षु"
    },
    {
        "sno": 27,
        "slokaNo": 32,
        "sloka": "उपजीवति स्म सततं दधतः परिमुग्धतां"
    },
    {
        "sno": 28,
        "slokaNo": 65,
        "sloka": "उपताप्यमानमलघूष्णिमभिः श्वसितैः"
    },
    {
        "sno": 29,
        "slokaNo": 72,
        "sloka": "उपनेतुमुन्नतिमतेव दिवं कुचयोर्युगेन"
    },
    {
        "sno": 30,
        "slokaNo": 5,
        "sloka": "उपसन्ध्यमास्त तनु सानुमतः शिखरेषु"
    },
    {
        "sno": 31,
        "slokaNo": 42,
        "sloka": "ककुभां मुखानि सहसोज्ज्वलयन् दधदाकुलत्व"
    },
    {
        "sno": 32,
        "slokaNo": 45,
        "sloka": "कदलीप्रकाण्डरुचिरोरुतरौ जघनस्थलीपरिसरे"
    },
    {
        "sno": 33,
        "slokaNo": 75,
        "sloka": "कररुद्धनीवि दयितोपगतौ गलितं"
    },
    {
        "sno": 34,
        "slokaNo": 27,
        "sloka": "कलया तुषारकिरणस्य पुरः परिमन्दभिन्न"
    },
    {
        "sno": 35,
        "slokaNo": 20,
        "sloka": "किमलम्बताऽम्बरविलग्ननमधः किमवर्धतो"
    },
    {
        "sno": 36,
        "slokaNo": 67,
        "sloka": "कुसुमादपि स्मितदृशः सुतरां सुकुमारमङ्ग"
    },
    {
        "sno": 37,
        "slokaNo": 2,
        "sloka": "गतया पुरः प्रतिगवाक्षमुखं दधती रतेन"
    },
    {
        "sno": 38,
        "slokaNo": 8,
        "sloka": "गतवत्यराजत जपाकुसुमस्तबकद्युतौ"
    },
    {
        "sno": 39,
        "slokaNo": 80,
        "sloka": "तदयुक्तमङ्ग तव विश्वसृजा न कृतं"
    },
    {
        "sno": 40,
        "slokaNo": 60,
        "sloka": "तदुपेत्य मा स्म तमुपालभथाः किल दोषमस्य"
    },
    {
        "sno": 41,
        "slokaNo": 64,
        "sloka": "तव सा कथासु परिघट्टयति श्रवणं यदङ्गुलि"
    },
    {
        "sno": 42,
        "slokaNo": 23,
        "sloka": "ददृशेऽपि भास्कररुचाऽह्नि न यः स तमीं तमोभि"
    },
    {
        "sno": 43,
        "slokaNo": 66,
        "sloka": "दधति स्फुटं रतिपतेरिषवः शिततां"
    },
    {
        "sno": 44,
        "slokaNo": 86,
        "sloka": "दधत्युरोजद्वयमुर्वशीतलं भुवो गतेव"
    },
    {
        "sno": 45,
        "slokaNo": 57,
        "sloka": "दयिताय मानपरयाऽपरया त्वरितं यया"
    },
    {
        "sno": 46,
        "slokaNo": 70,
        "sloka": "दयिताहृतस्य युवभिर्मनसः परिमूढता"
    },
    {
        "sno": 47,
        "slokaNo": 17,
        "sloka": "दिवसोऽनुमित्रमगमद्विलयं किमिहाऽऽस्यते बत"
    },
    {
        "sno": 48,
        "slokaNo": 34,
        "sloka": "दिवसं भृशोष्णरुचिपादहतां रुदतीमिवा"
    },
    {
        "sno": 49,
        "slokaNo": 9,
        "sloka": "द्रुतशातकुम्भनिभमंशुमतो वपुरर्धमग्न"
    },
    {
        "sno": 50,
        "slokaNo": 49,
        "sloka": "ध्रुवमागताः प्रतिहतिं कठिने मदनेषवः"
    },
    {
        "sno": 51,
        "slokaNo": 56,
        "sloka": " न च मेऽवगच्छति यथा लघुतां करुणां यथा च"
    },
    {
        "sno": 52,
        "slokaNo": 61,
        "sloka": "ननु सन्दिशेति सुदृशोदितया त्रपया न"
    },
    {
        "sno": 53,
        "slokaNo": 50,
        "sloka": "न मनोरमास्वपि विशेषविदां निरचेष्ट योग्य"
    },
    {
        "sno": 54,
        "slokaNo": 7,
        "sloka": "नवकुङ्कुमाऽरुणपयोधरया स्वकराऽवसक्त"
    },
    {
        "sno": 55,
        "slokaNo": 28,
        "sloka": "नवचन्द्रिकाकुसुमकीर्णतमःकबरीभृतो"
    },
    {
        "sno": 56,
        "slokaNo": 81,
        "sloka": "न विभावयत्यनिशमक्षिगतामपि मां"
    },
    {
        "sno": 57,
        "slokaNo": 52,
        "sloka": "निजपाणिपल्लवतलस्खलनादभिनासिकाविवर"
    },
    {
        "sno": 58,
        "slokaNo": 71,
        "sloka": "निपपात सम्भ्रमभृतः श्रवणादसितभ्रुवः"
    },
    {
        "sno": 59,
        "slokaNo": 16,
        "sloka": "निलयः श्रियः सततमेतदिति प्रथितं यदेव"
    },
    {
        "sno": 60,
        "slokaNo": 18,
        "sloka": "पतिते पतङ्गमृगराजि निजप्रतिबिम्बरोषित"
    },
    {
        "sno": 61,
        "slokaNo": 78,
        "sloka": "परिमन्थराभिरलघूरुभरादधिवेश्म पत्युरुप"
    },
    {
        "sno": 62,
        "slokaNo": 76,
        "sloka": "पिदधानमन्वगुपगम्य दृशौ ब्रुवते जनाय"
    },
    {
        "sno": 63,
        "slokaNo": 55,
        "sloka": "प्रणयप्रकाशनविदो मधुराः सुतरामभीष्टजन"
    },
    {
        "sno": 64,
        "slokaNo": 35,
        "sloka": "प्रतिकामिनीति ददृशुश्चकिताः स्मरजन्मघर्म"
    },
    {
        "sno": 65,
        "slokaNo": 6,
        "sloka": "प्रतिकूलतामुपगते हि विधौ विफलत्वमेति"
    },
    {
        "sno": 66,
        "slokaNo": 58,
        "sloka": "प्रतिभिद्य कान्तमपराधकृतं यदि तावदस्य"
    },
    {
        "sno": 67,
        "slokaNo": 29,
        "sloka": "प्रथमं कलाभवदथार्धमथो हिमदीधिति"
    },
    {
        "sno": 68,
        "slokaNo": 62,
        "sloka": "ब्रुवते स्म दूत्य उपसृत्य नरान्नरवत्प्रगल्भ"
    },
    {
        "sno": 69,
        "slokaNo": 48,
        "sloka": "भजते विदेशमधिकेन जितस्तदनुप्रवेशमथवा"
    },
    {
        "sno": 70,
        "slokaNo": 39,
        "sloka": "भवनोदरेषु परिमन्दतया शयितोऽलसः"
    },
    {
        "sno": 71,
        "slokaNo": 79,
        "sloka": "मधुरोन्नतभ्रु ललितं च दृशोः सकरप्रयोग"
    },
    {
        "sno": 72,
        "slokaNo": 63,
        "sloka": "मम रूपकीर्तिमहरद्भुवि यस्तदनुप्रविष्ट"
    },
    {
        "sno": 73,
        "slokaNo": 41,
        "sloka": "युगपद्विकासमुदयाद्गमिते शशिनः शिलीमुख"
    },
    {
        "sno": 74,
        "slokaNo": 33,
        "sloka": "रजनीमवाप्य रुचमाप शशी सपदि"
    },
    {
        "sno": 75,
        "slokaNo": 13,
        "sloka": "रुचिधाम्नि भर्तरि भृशं विमलाः परलोक"
    },
    {
        "sno": 76,
        "slokaNo": 51,
        "sloka": "वपुरन्वलिप्त परिरम्भसुखव्यवधानभीरुकतया"
    },
    {
        "sno": 77,
        "slokaNo": 25,
        "sloka": "वसुधाऽन्तनिःसृतमिवाहिपतेः पटलं फणामणि"
    },
    {
        "sno": 78,
        "slokaNo": 53,
        "sloka": "विधृते दिवा सवयसा च पुरः परिपूर्णमण्डल"
    },
    {
        "sno": 79,
        "slokaNo": 3,
        "sloka": "विरलातपच्छविरनुष्णवपुः परितो विपाण्डु"
    },
    {
        "sno": 80,
        "slokaNo": 26,
        "sloka": "विशदप्रभापरिगतं विबभावुदयाचलव्यवहितेन्दु"
    },
    {
        "sno": 81,
        "slokaNo": 68,
        "sloka": " विषतां निषेवितमपक्रियया समुपैति सर्वमिति"
    },
    {
        "sno": 82,
        "slokaNo": 14,
        "sloka": "विहिताञ्जलिर्जनतया दधती विकसत्कुसुम्भकुसुम"
    },
    {
        "sno": 83,
        "slokaNo": 19,
        "sloka": "व्यसरन्नु भूधरगुहान्तरतः पटलं बहिर्बहल"
    },
    {
        "sno": 84,
        "slokaNo": 44,
        "sloka": "सममेकमेव दधतुः सुतनोरु हारभूषण"
    },
    {
        "sno": 85,
        "slokaNo": 85,
        "sloka": "सुदृशः सरसव्यलीकतप्तस्तरसाश्लिष्टवतः"
    },
    {
        "sno": 86,
        "slokaNo": 21,
        "sloka": "स्थगिताम्बरक्षितितले परितस्तिमिरे जनस्य"
    },
    {
        "sno": 87,
        "slokaNo": 47,
        "sloka": "स्फुरदुज्ज्वलाधरदलैर्विलसद्दशनांशुकेशर"
    }
]