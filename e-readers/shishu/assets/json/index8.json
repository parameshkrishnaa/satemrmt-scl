[
    {
        "sno": 1,
        "slokaNo": 52,
        "sloka": "अन्यूनं गुणममृतस्य धारयन्ती सम्फुल्लस्फुरित"
    },
    {
        "sno": 2,
        "slokaNo": 25,
        "sloka": "आकृष्टप्रतनुवपुर्लतैस्तरद्भिस्तस्याऽम्भस्तदथ"
    },
    {
        "sno": 3,
        "slokaNo": 10,
        "sloka": "आघ्राय श्रमजमनिन्द्यगन्धबन्धुं निःश्वास"
    },
    {
        "sno": 4,
        "slokaNo": 27,
        "sloka": "आदातुं दयितमिवाऽवगाढमारादूर्मीणां ततिभि"
    },
    {
        "sno": 5,
        "slokaNo": 36,
        "sloka": "आनन्दं दधति मुखे करोदकेन श्यामाया"
    },
    {
        "sno": 6,
        "slokaNo": 22,
        "sloka": "आनाभेः सरसि नतभ्रुवाऽवगाढे चापल्यादथ"
    },
    {
        "sno": 7,
        "slokaNo": 45,
        "sloka": "आबद्धप्रचुरपरार्ध्यकिङ्किणीको रामाणामनवरतोदगाह"
    },
    {
        "sno": 8,
        "slokaNo": 61,
        "sloka": "आमृष्टास्तिलकरुचः स्रजोनिरस्ता नीरक्तं"
    },
    {
        "sno": 9,
        "slokaNo": 11,
        "sloka": "आयान्त्यां निजयुवतौ वनात्सशङ्कं"
    },
    {
        "sno": 10,
        "slokaNo": 1,
        "sloka": "आयासादलघुतरस्तनैः स्वनद्भिः श्रान्तानाम"
    },
    {
        "sno": 11,
        "slokaNo": 54,
        "sloka": "आरुढः पतित इति स्वसम्भवोऽपि स्वच्छानां"
    },
    {
        "sno": 12,
        "slokaNo": 67,
        "sloka": "आर्द्रत्वादतिशयिनीमुपेयिवद्भिः संसक्तिं भृशमपि"
    },
    {
        "sno": 13,
        "slokaNo": 12,
        "sloka": "आलापैस्तुलितरवाणि माधवीनां माधुर्यादमल"
    },
    {
        "sno": 14,
        "slokaNo": 19,
        "sloka": "आसीना तटभुवि सस्मितेन भर्त्रा"
    },
    {
        "sno": 15,
        "slokaNo": 16,
        "sloka": "आस्कन्दन् कथमपि योषितो न यावद्भीमत्यः"
    },
    {
        "sno": 16,
        "slokaNo": 50,
        "sloka": "आस्माकी युवतिदृशामसौ तनोतिच्छायेव"
    },
    {
        "sno": 17,
        "slokaNo": 71,
        "sloka": "इति धौतपुरन्ध्रिमत्सरान् सरसि मज्जनेन श्रियमाप्तवतो"
    },
    {
        "sno": 18,
        "slokaNo": 14,
        "sloka": "उत्क्षिप्तस्फुटितसरोरुहार्घ्यमुच्चैः सस्नेहं विहग"
    },
    {
        "sno": 19,
        "slokaNo": 31,
        "sloka": "उत्तुङ्गादनिलचलांशुकास्तटान्ताच्चेतोभिः सह"
    },
    {
        "sno": 20,
        "slokaNo": 37,
        "sloka": "उद्वीक्ष्य प्रियकरकुड्मलाऽपविद्धैर्वक्षोजद्वय"
    },
    {
        "sno": 21,
        "slokaNo": 44,
        "sloka": "उद्वोढुं कनकविभूषणान्यशक्तः सध्रीचा"
    },
    {
        "sno": 22,
        "slokaNo": 28,
        "sloka": "उन्निद्रप्रियकमनोरमं रमण्याः संरेजे सरसि"
    },
    {
        "sno": 23,
        "slokaNo": 4,
        "sloka": "एकस्यास्तपनकरैः करालिताया बिभ्राणः"
    },
    {
        "sno": 24,
        "slokaNo": 56,
        "sloka": "कस्याश्चिन्मुखमनु धौतपत्रलेखं व्यातेने"
    },
    {
        "sno": 25,
        "slokaNo": 23,
        "sloka": "कान्तानां कुवलयमप्यपास्तमक्ष्णोः शोभाभिर्न"
    },
    {
        "sno": 26,
        "slokaNo": 29,
        "sloka": "किं तावत्सरसि सरोजमेतदारादाहोस्विन्मुख"
    },
    {
        "sno": 27,
        "slokaNo": 38,
        "sloka": "कुर्वद्भिर्मुखरुचिमुज्ज्वलामजस्रम् यैस्तोयरसिञ्चत"
    },
    {
        "sno": 28,
        "slokaNo": 7,
        "sloka": "गच्छन्तीरलसमवेक्ष्य विस्मयिन्यस्तास्तन्वीर्न विदधिरे"
    },
    {
        "sno": 29,
        "slokaNo": 26,
        "sloka": "गाम्भीर्यं दधदपि रन्तुमङ्गनाभिः सङ्क्षोभं"
    },
    {
        "sno": 30,
        "slokaNo": 17,
        "sloka": "ताः पूर्वं सचकितमागमय्य गाधं कृत्वाऽथो"
    },
    {
        "sno": 31,
        "slokaNo": 21,
        "sloka": "तिष्ठन्तं पयसि पुमांसमंसमात्रे तद्दघ्नं"
    },
    {
        "sno": 32,
        "slokaNo": 24,
        "sloka": "त्रस्यन्ती चलशफरीविघट्टितोरुर्वामोरूरतिशयमाप"
    },
    {
        "sno": 33,
        "slokaNo": 55,
        "sloka": "दन्तानामधरमयावकं पदानि प्रत्यग्रास्तनु"
    },
    {
        "sno": 34,
        "slokaNo": 64,
        "sloka": "दिव्यानामपि कृतविस्मयां पुरस्तादम्भस्तः स्फुरदरविन्दचारु"
    },
    {
        "sno": 35,
        "slokaNo": 47,
        "sloka": "नारीभिर्गुरुजघनस्थलाहतानामास्यश्रीविजित"
    },
    {
        "sno": 36,
        "slokaNo": 15,
        "sloka": "नित्याया निजवसतेर्निरासिरे यद्रागेण"
    },
    {
        "sno": 37,
        "slokaNo": 51,
        "sloka": " निर्धौते सति हरिचन्दने जलौघैरापाण्डोर्गत"
    },
    {
        "sno": 38,
        "slokaNo": 3,
        "sloka": "नीरन्ध्रद्रुमशिशिरां भुवं व्रजन्तीः साशङ्कं"
    },
    {
        "sno": 39,
        "slokaNo": 20,
        "sloka": "नेच्छन्ती समममुना सरोऽवगाढुं रोधस्तः"
    },
    {
        "sno": 40,
        "slokaNo": 46,
        "sloka": "पर्यच्छे सरसि हृतेंऽशुके पयोभिर्लोलाक्षे"
    },
    {
        "sno": 41,
        "slokaNo": 68,
        "sloka": "प्रत्यंसं विलुलितमूर्धजा चिराय स्नानार्द्रं"
    },
    {
        "sno": 42,
        "slokaNo": 49,
        "sloka": "प्रभ्रष्टैः सरभसमम्भसोऽवगाहक्रीडाभिर्विदलित"
    },
    {
        "sno": 43,
        "slokaNo": 40,
        "sloka": "प्रेम्णोरः प्रणयिनि सिञ्चति प्रियायाः सन्तापं"
    },
    {
        "sno": 44,
        "slokaNo": 59,
        "sloka": "फेनानामुरसिरुहेषु हारलीला चेलश्रीर्जघनतलेषु"
    },
    {
        "sno": 45,
        "slokaNo": 60,
        "sloka": "भ्रश्यद्भिर्जलमभि भूषणैर्वधूनामङ्गेभ्यो गुरुभिरमज्जि"
    },
    {
        "sno": 47,
        "slokaNo": 13,
        "sloka": "मुग्धायाः स्मरललितेषु चक्रवाक्या निःशङ्कं"
    },
    {
        "sno": 48,
        "slokaNo": 9,
        "sloka": "मुक्ताभिः सलिलरयाऽस्तशुक्तिपेशीमुक्ताभिः कृतरुचि"
    },
    {
        "sno": 49,
        "slokaNo": 2,
        "sloka": "यान्तीनां सममसितभ्रुवां नतत्वादंसानां महति"
    },
    {
        "sno": 50,
        "slokaNo": 33,
        "sloka": "योग्यस्य त्रिनयनलोचनाऽनलार्चिर्निदग्ध"
    },
    {
        "sno": 51,
        "slokaNo": 58,
        "sloka": "यो बाह्यः स खलु जलैर्निरासि रागो यश्चित्ते"
    },
    {
        "sno": 52,
        "slokaNo": 39,
        "sloka": "रागाऽन्धीकृतनयनेन नामधेयव्यक्त्यासादभिमुख"
    },
    {
        "sno": 53,
        "slokaNo": 57,
        "sloka": "वक्षोभ्यो घनमनुलेपनं यदूनामुत्तंसानहरत"
    },
    {
        "sno": 54,
        "slokaNo": 66,
        "sloka": "वासांसि न्यवसत यानि योषितस्ताः शुभ्राऽभ्र"
    },
    {
        "sno": 55,
        "slokaNo": 41,
        "sloka": "सङ्क्रान्तं प्रियतमवक्षसोऽङ्गरागं साध्वस्याः सरसि"
    },
    {
        "sno": 56,
        "slokaNo": 18,
        "sloka": "सङ्क्षोभं पयसि मुहुर्महेभकुम्भश्रीभाजा"
    },
    {
        "sno": 57,
        "slokaNo": 6,
        "sloka": "संस्पर्शप्रभवसुखोपचीयमाने सर्वाङ्गे"
    },
    {
        "sno": 58,
        "slokaNo": 43,
        "sloka": "सिक्तायाः क्षणमभिषिच्य पूर्वमन्यामन्यस्याः प्रणयवता"
    },
    {
        "sno": 59,
        "slokaNo": 34,
        "sloka": "सिञ्चन्त्याः कथमपि बाहुमुन्नमय्य प्रेयांसं"
    },
    {
        "sno": 60,
        "slokaNo": 69,
        "sloka": "सीमन्तं निजमनुबध्नती कराभ्यामालक्ष्यस्तनतटबाहु"
    },
    {
        "sno": 61,
        "slokaNo": 48,
        "sloka": "सौगन्ध्यं दधदपि काममङ्गनानां दूरत्वाद्गत"
    },
    {
        "sno": 62,
        "slokaNo": 53,
        "sloka": "स्नान्तीनां बृहृदमलोदबिन्दुचित्रौ रेजाते"
    },
    {
        "sno": 63,
        "slokaNo": 35,
        "sloka": "स्निह्यन्ती दृशमपरा निधाय पूर्णं मूर्तेन"
    },
    {
        "sno": 64,
        "slokaNo": 70,
        "sloka": "स्वच्छाम्भःस्नपनविधौतमङ्गमोष्ठस्ताम्बूलद्युतिविशदो "
    },
    {
        "sno": 65,
        "slokaNo": 5,
        "sloka": "स्वं रागादुपरि वितन्वतोत्तरीयं कान्तेन"
    },
    {
        "sno": 66,
        "slokaNo": 62,
        "sloka": "शीतार्तिं बलवदुपेयुषेव नीरैरासेकाच्छिशिरसमीर"
    },
    {
        "sno": 67,
        "slokaNo": 30,
        "sloka": "श्रृङ्गाणि द्रुतकनकोज्ज्वलनानि गन्धाः"
    },
    {
        "sno": 68,
        "slokaNo": 63,
        "sloka": "श्च्योतद्भिः समधिकमात्तमङ्गसङ्गात्लावण्यं तनुमदिवाऽम्बु"
    },
    {
        "sno": 69,
        "slokaNo": 8,
        "sloka": "श्रीमद्भिर्जितपुलिनानि माधवीनामारोहैर्निबिड"
    },
    {
        "sno": 70,
        "slokaNo": 65,
        "sloka": "श्लक्ष्णं यत्परिहितमेतयोः किलाऽन्तर्धानार्थं"
    },
    {
        "sno": 71,
        "slokaNo": 42,
        "sloka": "हूतायाः प्रतिसखि कामिनाऽन्यनाम्ना ह्रीमत्याः"
    }
]