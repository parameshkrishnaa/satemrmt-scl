[
    {
        "sno": 1,
        "slokaNo": 39,
        "sloka": "अम्भश्च्युतः कोमलरत्नराशीनपांनिधिः फेन"
    },
    {
        "sno": 2,
        "slokaNo": 26,
        "sloka": "अथोच्चकैस्तोरणसङ्गभङ्गभयावनम्रीकृत"
    },
    {
        "sno": 3,
        "slokaNo": 30,
        "sloka": "अवेक्षितानायतवल्गमग्रे तुरङ्गिभिर्यत्न"
    },
    {
        "sno": 4,
        "slokaNo": 15,
        "sloka": "आकर्षतेवोर्ध्वमतिक्रशीयानत्युन्नतत्वात्कुच"
    },
    {
        "sno": 5,
        "slokaNo": 72,
        "sloka": "आश्लिष्टभूमिं रसितारमुच्चैर्लोलद्भुजाकार"
    },
    {
        "sno": 6,
        "slokaNo": 80,
        "sloka": "उत्तालतालीवनसम्प्रवृत्तसमीरसीमन्तित"
    },
    {
        "sno": 7,
        "slokaNo": 77,
        "sloka": " उत्पिसवोऽन्तर्नदभर्तुरुच्चैर्गरीयसा निःश्वसिता"
    },
    {
        "sno": 8,
        "slokaNo": 79,
        "sloka": "   उत्सङ्गिताम्भःकणको नभस्वानुदन्वतः"
    },
    {
        "sno": 9,
        "slokaNo": 75,
        "sloka": "उद्धृत्य मेघैस्तत एव तोयमर्थं मुनीन्द्रैरिव"
    },
    {
        "sno": 10,
        "slokaNo": 32,
        "sloka": "उपेयुषो वर्त्म निरन्तराभिरसौ निरुच्छ्वास"
    },
    {
        "sno": 11,
        "slokaNo": 8,
        "sloka": " उभौ यदि व्योम्नि पृथक् प्रवाहावाकाशगङ्गा"
    },
    {
        "sno": 12,
        "slokaNo": 13,
        "sloka": "  कपाटविस्तीर्णमनोरमोरःस्थलस्थितश्रीललनस्य"
    },
    {
        "sno": 13,
        "slokaNo": 60,
        "sloka": " कला दधानः सकलाः स्वभाभिरुद्भासयन्"
    },
    {
        "sno": 14,
        "slokaNo": 44,
        "sloka": "कान्तेन्दुकान्तोपलकुट्टिमेषु प्रतिक्षपम् हर्म्यतलेषु"
    },
    {
        "sno": 15,
        "slokaNo": 41,
        "sloka": "कुतूहलेनेव जवादुपेत्य प्राकारभित्त्या"
    },
    {
        "sno": 16,
        "slokaNo": 34,
        "sloka": "कृतास्पदा भूमिभृतां सहस्रैरुदन्वदम्भःपरिवीत"
    },
    {
        "sno": 17,
        "slokaNo": 1,
        "sloka": "कौबेरदिग्भागमपास्य मार्गमागस्त्यमुष्णांशु"
    },
    {
        "sno": 18,
        "slokaNo": 52,
        "sloka": "क्षितिप्रतिष्ठोऽपि मुखारविन्दैर्वधूजनश्चन्द्र"
    },
    {
        "sno": 19,
        "slokaNo": 59,
        "sloka": "क्षुण्णं यदन्तःकरणेन वृक्षाः फलन्ति"
    },
    {
        "sno": 20,
        "slokaNo": 49,
        "sloka": "गोपानसीषु क्षणमास्थितानामालम्बिभिश्चन्द्रकिणां"
    },
    {
        "sno": 21,
        "slokaNo": 51,
        "sloka": "चिक्रंसया कृत्रिमपत्रिपङ्क्तेः कपोतपालीषु"
    },
    {
        "sno": 22,
        "slokaNo": 4,
        "sloka": "चित्राभिरस्योपरिमौलिभाजां भाभिर्मणी"
    },
    {
        "sno": 23,
        "slokaNo": 56,
        "sloka": "छन्नेष्वपि स्पष्टतरेषु यत्र स्वच्छानि नारी"
    },
    {
        "sno": 24,
        "slokaNo": 2,
        "sloka": "जगत्पवित्रैरपि तं न पादैः स्प्रष्टुं जगत् "
    },
    {
        "sno": 25,
        "slokaNo": 6,
        "sloka": "तमङ्गदे मन्दरकूटकोटिव्याघट्टनोत्तेज"
    },
    {
        "sno": 26,
        "slokaNo": 78,
        "sloka": " तमागतं वीक्ष्य युगान्तबन्धुमुत्सङ्गशय्या "
    },
    {
        "sno": 27,
        "slokaNo": 17,
        "sloka": " तस्यातसीसूनसमानभासो भ्राम्यन्"
    },
    {
        "sno": 28,
        "slokaNo": 5,
        "sloka": "तस्योल्लसत्काञ्चनकुण्डलाग्रप्रत्युप्तगारुत्म"
    },
    {
        "sno": 29,
        "slokaNo": 64,
        "sloka": "तामीक्षमाणः स पुरं पुरस्तात्प्रापत्प्रतोलीमतुल"
    },
    {
        "sno": 30,
        "slokaNo": 82,
        "sloka": " तुरगशताकुलस्य परितः परमेकतुरङ्गजन्मनः"
    },
    {
        "sno": 31,
        "slokaNo": 9,
        "sloka": " तेनाम्भसां सारमयः पयोधेर्दध्रे मणिर्दीधिति"
    },
    {
        "sno": 32,
        "slokaNo": 35,
        "sloka": "त्वष्टुः सदाभ्यासगृहीतशिल्पविज्ञान"
    },
    {
        "sno": 33,
        "slokaNo": 31,
        "sloka": " दिदृक्षमाणाः प्रतिरथ्यमीयुर्मुरारिमारा"
    },
    {
        "sno": 34,
        "slokaNo": 23,
        "sloka": "  ध्वजाग्रधामा ददृशेऽथ शौरेः सङ्क्रान्त"
    },
    {
        "sno": 35,
        "slokaNo": 19,
        "sloka": " न केवलं यः स्वतया मुरारेरनन्यसाधार"
    },
    {
        "sno": 36,
        "slokaNo": 20,
        "sloka": "न नीतमन्येन नतिं कदाचित् कर्णान्तिक"
    },
    {
        "sno": 37,
        "slokaNo": 28,
        "sloka": " न लङ्घयामास महाजनानां शिरांसि"
    },
    {
        "sno": 38,
        "slokaNo": 67,
        "sloka": "   निरन्तरालेपि विमच्यमाने दूरं पथि"
    },
    {
        "sno": 39,
        "slokaNo": 29,
        "sloka": "निरुध्यमाना यदुभिः कथञ्चिन्मुहुर्यदु"
    },
    {
        "sno": 40,
        "slokaNo": 7,
        "sloka": "निसर्गरक्तैर्वलयावनद्धताम्राश्मरश्मिच्छुरि"
    },
    {
        "sno": 41,
        "slokaNo": 62,
        "sloka": " निषेव्यमाणेन शिवैर्मरुद्भिरध्यास्यमाना"
    },
    {
        "sno": 42,
        "slokaNo": 58,
        "sloka": "परस्परस्पर्धिपरार्ध्यरूपाः पौरस्त्रियो यत्र"
    },
    {
        "sno": 43,
        "slokaNo": 70,
        "sloka": "पारेजलं नीरनिधेरपश्यन् मुरारिरानी"
    },
    {
        "sno": 44,
        "slokaNo": 73,
        "sloka": " पीत्वा जलानां निधिनातिगार्ध्याद् वृद्धिं गते"
    },
    {
        "sno": 45,
        "slokaNo": 65,
        "sloka": "प्रजा इवाङ्गादरविन्दनाभेः शम्भोर्जटा"
    },
    {
        "sno": 46,
        "slokaNo": 21,
        "sloka": "प्रवृद्धमन्द्राम्बुदधीरनादः कृष्णार्णवाभ्यर्ण"
    },
    {
        "sno": 47,
        "slokaNo": 12,
        "sloka": "प्रसाधितस्यास्य मधुद्विषोऽभूदन्यैव"
    },
    {
        "sno": 48,
        "slokaNo": 14,
        "sloka": "प्राणच्छिदां दैत्यपतेर्नखानामुपेयुषां"
    },
    {
        "sno": 49,
        "slokaNo": 69,
        "sloka": "बलोर्मिभिस्तत्क्षणहीयमानरथ्याभुजाया"
    },
    {
        "sno": 50,
        "slokaNo": 61,
        "sloka": "बाणाहवव्याहतशम्भुशक्तेरासत्ति"
    },
    {
        "sno": 51,
        "slokaNo": 50,
        "sloka": "बृहत्तुलैरप्यतुलैर्वितानमालापिनद्धैरपि"
    },
    {
        "sno": 52,
        "slokaNo": 33,
        "sloka": "मध्येसमुद्रं ककुभः पिशङ्गीर्या कुर्वती"
    },
    {
        "sno": 53,
        "slokaNo": 10,
        "sloka": "मुक्तामयं सारसनावलम्बि भाति"
    },
    {
        "sno": 54,
        "slokaNo": 3,
        "sloka": "मृणालसूत्रामलमन्तरेण स्थितश्चल"
    },
    {
        "sno": 55,
        "slokaNo": 40,
        "sloka": "यच्छालमुत्तुङ्गतया विजेतुं दूरादुदस्थीयत"
    },
    {
        "sno": 56,
        "slokaNo": 25,
        "sloka": "यतः स भर्ता जगतां जगाम धर्त्रा धरित्र्याः"
    },
    {
        "sno": 57,
        "slokaNo": 42,
        "sloka": "यदङ्गनारूपसरूपतायाः कञ्चिद् गुणं भेदक"
    },
    {
        "sno": 58,
        "slokaNo": 57,
        "sloka": "यस्यामजिह्या महतीमपङ्काः सीमानम"
    },
    {
        "sno": 59,
        "slokaNo": 46,
        "sloka": "यस्यामतिश्लक्ष्णतया गृहेषु विधातुमालेख्य"
    },
    {
        "sno": 60,
        "slokaNo": 37,
        "sloka": "यस्याश्चलद्वारिधिवारिवीचिच्छटोच्छलच्छ"
    },
    {
        "sno": 61,
        "slokaNo": 16,
        "sloka": " यां यां प्रियः प्रैक्षत कातराक्षीं सा सा"
    },
    {
        "sno": 62,
        "slokaNo": 24,
        "sloka": "यियासतस्तस्य महीन्द्ररन्ध्रभिदापटीयान्"
    },
    {
        "sno": 63,
        "slokaNo": 55,
        "sloka": "रतान्तरे यत्र गृहान्तरेषु वितर्दिनिर्यूह"
    },
    {
        "sno": 64,
        "slokaNo": 45,
        "sloka": " रतौ ह्रिया यत्र निशम्य दीपञ्जालागताभ्योऽधि"
    },
    {
        "sno": 65,
        "slokaNo": 36,
        "sloka": "रथाङ्गभर्त्रेऽभिनवं वराय यस्याः पितेव"
    },
    {
        "sno": 66,
        "slokaNo": 53,
        "sloka": "रम्या इति प्राप्तवतीः पताका रागं विविक्ता इति"
    },
    {
        "sno": 67,
        "slokaNo": 22,
        "sloka": "रराज सम्पादकमिष्टसिद्धेः सर्वासु दिक्ष्वप्रति"
    },
    {
        "sno": 68,
        "slokaNo": 71,
        "sloka": "लक्ष्मीभृतोऽम्भोधितटाधिवासान् द्रुमानसौ"
    },
    {
        "sno": 69,
        "slokaNo": 81,
        "sloka": "लवङ्गमालाकलितावतंसास्ते नारिकेल"
    },
    {
        "sno": 70,
        "slokaNo": 38,
        "sloka": "वणिक्पथे पूगकृतानि यत्र भ्रमागतेरम्बु"
    },
    {
        "sno": 71,
        "slokaNo": 76,
        "sloka": "विक्रीय दिश्यानि धनान्युरूणि द्वैप्यानसावुत्त"
    },
    {
        "sno": 72,
        "slokaNo": 18,
        "sloka": "विरोधिनां विग्रहभेददक्षा मूर्तेव शक्तिः"
    },
    {
        "sno": 73,
        "slokaNo": 68,
        "sloka": "शनैरनीयन्त रयात्पतन्तो रथाः क्षितिं"
    },
    {
        "sno": 74,
        "slokaNo": 48,
        "sloka": "शुकाङ्गनीलोपलनिर्मितानां लिप्तेषु भासा"
    },
    {
        "sno": 75,
        "slokaNo": 27,
        "sloka": "श्यामारुणैर्वारणदानतोयैरालोडिताः"
    },
    {
        "sno": 76,
        "slokaNo": 66,
        "sloka": "श्लिष्यद्भिरन्योन्यमुखाग्रसङ्गस्खलत्खलीनं"
    },
    {
        "sno": 77,
        "slokaNo": 11,
        "sloka": " स इन्द्रनीलस्थलनीलमूर्ती रराज कर्चूर"
    },
    {
        "sno": 78,
        "slokaNo": 74,
        "sloka": "साटोपमुर्वीमनिशं नदन्तो यैः प्लावयिष्यन्ति"
    },
    {
        "sno": 79,
        "slokaNo": 47,
        "sloka": "सावर्ण्यभाजां प्रतिमागतानां लक्ष्यैः स्मरापाण्डु"
    },
    {
        "sno": 80,
        "slokaNo": 54,
        "sloka": "सुगन्धितामप्रतियत्नपूर्वां बिभ्रन्ति यत्र प्रमदाय"
    },
    {
        "sno": 81,
        "slokaNo": 63,
        "sloka": "स्निग्धाञ्जनश्यामरुचिः सुवृत्तो वध्वा इवाध्वंसित"
    },
    {
        "sno": 82,
        "slokaNo": 43,
        "sloka": "स्फुरत्तुषारांशुमरीचिजालैर्विनिह्नुताः स्फाटिक"
    }
]