[
    {
        "sno": 1,
        "slokaNo": 12,
        "sloka": "अखिद्यतासन्नमुदग्रतापं रविं "
    },
    {
        "sno": 2,
        "slokaNo": 41,
        "sloka": "अनतिचिरोज्झितस्य जलदेन चिरस्थित"
    },
    {
        "sno": 3,
        "slokaNo": 68,
        "sloka": "अनुकृतशिखरौघश्रीभिरभ्यागतोऽसौ त्वयि  "
    },
    {
        "sno": 4,
        "slokaNo": 53,
        "sloka": "अन्योऽन्यव्यतिकरचारुभिर्विचित्रैरत्र"
    },
    {
        "sno": 5,
        "slokaNo": 47,
        "sloka": "अपशङ्कमङ्कपरिवर्तनोचिताश्चलिताः पुरः"
    },
    {
        "sno": 6,
        "slokaNo": 29,
        "sloka": "अयमतिजरठाः प्रकामगुर्वीरलघुविलम्बि"
    },
    {
        "sno": 7,
        "slokaNo": 19,
        "sloka": "आच्छादितायतदिगम्बरमुच्चकैर्गामाक्रम्य"
    },
    {
        "sno": 8,
        "slokaNo": 52,
        "sloka": "आच्छाद्य पुष्पपटमेष महान्तमन्तरावर्तिभिर्"
    },
    {
        "sno": 9,
        "slokaNo": 34,
        "sloka": "आसादितस्य तमसा नियतेर्नियोगादाङ्क्षतः"
    },
    {
        "sno": 10,
        "slokaNo": 27,
        "sloka": "इतस्ततोऽस्मिन् विलसन्ति मेरोः समानवप्रे"
    },
    {
        "sno": 11,
        "slokaNo": 60,
        "sloka": "इह मुहुर्मुदितैः कलभैः रवः प्रतिदिशं क्रियते"
    },
    {
        "sno": 12,
        "slokaNo": 18,
        "sloka": "उच्चारणज्ञोऽथ गिरां दधानमुच्चारणत्पक्षि"
    },
    {
        "sno": 13,
        "slokaNo": 28,
        "sloka": "उच्चैर्महारजतराजिविराजिताऽसौ दुर्वर्णभित्तिरिह"
    },
    {
        "sno": 14,
        "slokaNo": 25,
        "sloka": "उत्क्षिप्तमुच्छ्रितसितांशुकरावलम्बैरुत्तम्भितोडुभिरतीवतरां"
    },
    {
        "sno": 15,
        "slokaNo": 20,
        "sloka": "उदयति विततोर्ध्वरश्मिरज्जावहिमरुचौ हिमधाम्नि"
    },
    {
        "sno": 16,
        "slokaNo": 26,
        "sloka": "एकत्र स्फटिकतटांशुभिन्ननीरा नीलाश्मद्युति"
    },
    {
        "sno": 17,
        "slokaNo": 59,
        "sloka": "एतस्मिन्नधिकपयःश्रियं वहन्त्यः संक्षोभं पवनभुवा"
    },
    {
        "sno": 18,
        "slokaNo": 33,
        "sloka": "कुशेशयैरत्र जलाशयोषिता मुदा रमन्ते"
    },
    {
        "sno": 19,
        "slokaNo": 23,
        "sloka": "कृत्वा पुंवत्त्पातमुच्चैर्भृगुभ्यो मूर्ध्नि ग्राव्णां"
    },
    {
        "sno": 20,
        "slokaNo": 3,
        "sloka": "क्रान्तं रुचा काञ्चनवप्रभाजा नवप्रभाजालभृतां"
    },
    {
        "sno": 21,
        "slokaNo": 5,
        "sloka": "क्वचिज्जलापायविपाण्डुराणि धौतोत्तरीय"
    },
    {
        "sno": 22,
        "slokaNo": 2,
        "sloka": "गुर्वीरजस्रं दृषदः समन्तादुपर्युपर्यम्बुमुचां"
    },
    {
        "sno": 23,
        "slokaNo": 6,
        "sloka": "छायां निजस्त्रीचटुलालसानां मदेन"
    },
    {
        "sno": 24,
        "slokaNo": 61,
        "sloka": "त्वक्साररन्ध्रपरिपूरणलब्धगीतिरस्मिन्नसौ"
    },
    {
        "sno": 25,
        "slokaNo": 50,
        "sloka": "दधति च विकसद्विचित्रकल्पद्रुमकुसुमैरभि"
    },
    {
        "sno": 26,
        "slokaNo": 66,
        "sloka": "दधद्भिरभितस्तटौ विकचवारिजाम्बू नदैर्विनोदित"
    },
    {
        "sno": 27,
        "slokaNo": 40,
        "sloka": "दन्तोज्ज्वलासु विमलोपलमेखलान्ताः सद्रत्नचित्र"
    },
    {
        "sno": 28,
        "slokaNo": 67,
        "sloka": "दर्पणनिर्मलासु पतिते घनतिमिरमुषि ज्योतिषि"
    },
    {
        "sno": 29,
        "slokaNo": 17,
        "sloka": "दृष्टोऽपि शैलः स मुहुर्मुरारेरपूर्ववद्विस्मय"
    },
    {
        "sno": 30,
        "slokaNo": 30,
        "sloka": "धूमाकरं दधति पुरःसौवर्णे वर्णेनाग्नेः सदृशि"
    },
    {
        "sno": 31,
        "slokaNo": 65,
        "sloka": "नवनगवनलेखाश्याममध्याभिराभिः स्फटिक"
    },
    {
        "sno": 32,
        "slokaNo": 1,
        "sloka": "निःश्वासधूमं सहरत्नभाभिर्भित्वोत्थितं"
    },
    {
        "sno": 33,
        "slokaNo": 22,
        "sloka": "पाश्चात्यभागमिह सानुषु सन्निषण्णाः पश्यन्ति"
    },
    {
        "sno": 34,
        "slokaNo": 49,
        "sloka": "प्राग्भागतः पतदिहेदमुपत्यकासु श्रृङ्गारितायत "
    },
    {
        "sno": 35,
        "slokaNo": 64,
        "sloka": "प्रालेयशीतमचलेश्वरमीश्वरोऽपि सान्द्रेभचर्म"
    },
    {
        "sno": 36,
        "slokaNo": 62,
        "sloka": "प्रीत्यै यूनां व्यवहिततपनाः प्रौढध्वान्तं दिनमिह"
    },
    {
        "sno": 37,
        "slokaNo": 16,
        "sloka": "फलद्भिरुष्णांशुकराभिमर्शात्कार्शानवं धाम"
    },
    {
        "sno": 38,
        "slokaNo": 38,
        "sloka": "बिम्बोष्ठं बहु मनुते तुरङ्गवक्त्रश्चुम्बन्तं मुखमिह"
    },
    {
        "sno": 39,
        "slokaNo": 63,
        "sloka": "भग्नो निवासोऽयमिहास्य पुष्पैः सदानतो येन"
    },
    {
        "sno": 40,
        "slokaNo": 46,
        "sloka": "भिन्नेषु रत्नकिरणैः किरणेष्विहेन्दोरुच्चावचैरुपगतेषु"
    },
    {
        "sno": 41,
        "slokaNo": 48,
        "sloka": "मधुकरविटपानमितास्तरुपङ्क्तीर्बिभ्रतोऽस्य"
    },
    {
        "sno": 42,
        "slokaNo": 56,
        "sloka": "मरकतमयमेदिनीषु भानोस्तरुविटपान्तरपातिनो"
    },
    {
        "sno": 43,
        "slokaNo": 44,
        "sloka": "मुक्तं मुक्तागौरमिह क्षीरमिवाभ्रैर्वापीष्वन्तर्लीन"
    },
    {
        "sno": 44,
        "slokaNo": 10,
        "sloka": "मुदे मुरारेरमरैः सुमेरोरानीय यस्योपचितस्य"
    },
    {
        "sno": 45,
        "slokaNo": 55,
        "sloka": "मैत्र्यादिचित्तपरिकर्मविदो विधाय क्लेशप्रहाणमिह"
    },
    {
        "sno": 46,
        "slokaNo": 11,
        "sloka": "यतः परार्ध्यानि भृतान्यनूनैः प्रस्थैर्मुहुर्भूरिभि"
    },
    {
        "sno": 47,
        "slokaNo": 13,
        "sloka": "यत्राधिरूढेन महीरुहोच्चैरुन्निद्रपुष्पाक्षिसहस्र"
    },
    {
        "sno": 48,
        "slokaNo": 15,
        "sloka": "यत्रोज्झिताभिर्मुहुरम्बुवाहैः समुन्नमद्भिः"
    },
    {
        "sno": 49,
        "slokaNo": 39,
        "sloka": "यदेतदस्यानुतटं विभाति वनं ततानेकतमाल"
    },
    {
        "sno": 50,
        "slokaNo": 45,
        "sloka": "या न ययौ प्रियमन्यवधूभ्यः सारतरागमना"
    },
    {
        "sno": 51,
        "slokaNo": 57,
        "sloka": "या बिभर्ति कलवल्लकीगुणस्वानमानमतिकालि"
    },
    {
        "sno": 52,
        "slokaNo": 9,
        "sloka": "राजीवराजीवशलोलभृङ्गं मुष्णन्तमुष्णं"
    },
    {
        "sno": 53,
        "slokaNo": 32,
        "sloka": "रुचिरचित्रतनूरुहशालिभिर्विचलितैः परितः"
    },
    {
        "sno": 54,
        "slokaNo": 35,
        "sloka": "वनस्पतिस्कन्धनिषण्णबालप्रवालहस्ताः"
    },
    {
        "sno": 55,
        "slokaNo": 42,
        "sloka": "वर्जयन्त्या जनैः सङ्गमेकान्ततस्तर्कयन्त्या सुखं"
    },
    {
        "sno": 56,
        "slokaNo": 21,
        "sloka": "वहति यः परितः कनकस्थलीः सहरिता लसमानन"
    },
    {
        "sno": 57,
        "slokaNo": 37,
        "sloka": "विद्वद्भिरागमपरैर्विवृतं कथञ्चिच्छ्रुत्वाऽपि दुर्ग्रहमनिश्चित"
    },
    {
        "sno": 58,
        "slokaNo": 14,
        "sloka": "विभिन्नवर्णा गरुडाग्रजेन सूर्यस्य रथ्याः परितः"
    },
    {
        "sno": 59,
        "slokaNo": 8,
        "sloka": " विलम्बिनीलोत्पलकर्णपूराः कपोलभित्तीरिव"
    },
    {
        "sno": 60,
        "slokaNo": 36,
        "sloka": "विहगाः कदम्बसुरभाविह गाः कलयन्त्यनुक्षण"
    },
    {
        "sno": 61,
        "slokaNo": 31,
        "sloka": "व्योमस्पृशः प्रथयता कलधौतभित्तीरुन्निद्रपुष्प"
    },
    {
        "sno": 62,
        "slokaNo": 43,
        "sloka": "सङ्कीर्णकीचकवनस्खलितैकबालविच्छेदकातरधिय"
    },
    {
        "sno": 63,
        "slokaNo": 54,
        "sloka": "समीरशिशिर शिरःसु वसतां सतां जवनिका निकाम"
    },
    {
        "sno": 64,
        "slokaNo": 51,
        "sloka": "सवधूकाः सुखिनोऽस्मिन्ननवरतममन्दरागतामर"
    },
    {
        "sno": 65,
        "slokaNo": 4,
        "sloka": "सहस्रसङ्ख्यैर्गगनं शिरोभिः पादैर्भुवं व्याप्य"
    },
    {
        "sno": 66,
        "slokaNo": 58,
        "sloka": "सायं शशाङ्ककिरणाहतचन्द्रकान्तनिस्यन्दिनीरनिकरेण"
    },
    {
        "sno": 67,
        "slokaNo": 7,
        "sloka": "स्कन्धाधिरूढोज्ज्वलनीलकण्ठानुर्वीरुहः श्लिष्ट"
    },
    {
        "sno": 68,
        "slokaNo": 24,
        "sloka": "स्थगयन्त्यमूः शमितचातकार्तस्वरा जलदास्तडित"
    }
]